ser/avatar/3883365/avatar.png\"></v-img>\n            </v-avatar>\n          </div>\n            <v-card-text class=\"text-center pb-0\">\n            <b>{{profile.attributes.firstName}} {{profile.attributes.lastName}}</b>\n            </v-card-text>        \n            <v-card-text class=\"text-center pt-0\">\n              <span>{{profile.attributes.email}}</span>\n            </v-card-text>\n            <div class=\"text-center py-5\">\n              <v-btn\n                class=\"text-capitalize\"\n                light\n              >\n                Manage Account {{ profile.id}}\n              </v-btn>\n            </div>\n            <v-divider></v-divider>\n            <div class=\"text-center py-5\">\n              <v-btn\n                class=\"text-capitalize\"\n                light\n                @click=\"signOut\"\n              >\n                Sign Out\n              </v-btn>\n            </div>\n            <v-divider></v-divider>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn\n            color=\"green darken-1\"\n            text\n            @click=\"dialog = false\"\n            class=\"text-capitalize\"\n          >\n            Privacy Policy\n          </v-btn>\n          <v-btn\n            color=\"green darken-1\"\n            text\n            @click=\"dialog = false\"\n            class=\"text-capitalize\"\n          >\n            Terms of Service\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-menu>\n    <!-- </v-dialog> -->\n    </v-col>\n  </v-row>\n</template>\n<script>\n  export default {\n    name : 'Dialog',\n    data () {\n      return {\n        dialog: false,\n        user: {\n          attributes : {\n            fullName : 'Patrick Nzambu',\n            email : 'patrick@gmail.com'\n          }\n        }\n      }\n    },\n    mounted()  {\n        this.$nextTick(() => this.getProfile())\n    },\n    computed: {\n      profile() {\n        return this.$store.state.auth.profile;\n      },\n    },\n    methods : {\n      getProfile() {\n        this.$store.dispatch('auth/profile').then(\n          profile => {\n              console.log(profile)\n          },\n          error => {\n              console.log(error)\n          }\n        )\n      },\n      signOut() {\n        this.$store.dispatch('auth/logout').then(\n        response => {\n          console.log(response.data)\n        },\n        error => {\n          console.log(error.response)\n        })\n      }\n    }\n  }\n</script>"]}]}