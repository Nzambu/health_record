             @click=\"toggleAddProperty\"                \n            >\n                <v-icon>mdi-plus-circle</v-icon> Property\n            </v-btn>   \n        </v-row> -->\n        <v-row justify=\"center\" v-if=\"addProperty\">\n            <v-col \n                cols=\"12\"\n                sm=\"12\"\n                md=\"12\"\n                lg=\"12\"\n            >\n                <v-stepper v-model=\"e1\" alt-labels>\n                    <!-- Wizard header -->\n                    <v-stepper-header>\n                        <v-stepper-step\n                            :complete=\"e1 > 1\"\n                            step=\"1\"\n                            @click=\"saveProperty\"\n                        >\n                            Name of step 1\n                        </v-stepper-step>\n\n                        <v-divider></v-divider>\n\n                        <v-stepper-step\n                            :complete=\"e1 > 2\"\n                            step=\"2\"\n                        >\n                            Name of step 2\n                        </v-stepper-step>\n\n                        <v-divider></v-divider>\n\n                        <v-stepper-step \n                            :rules=\"[() => false]\"\n                            step=\"3\"\n                        >\n                            Name of step 3\n                            <small>Alert message</small>\n                        </v-stepper-step>\n                    </v-stepper-header>\n                    <!--  The wizard contents -->\n                    <v-stepper-items>\n                    <v-stepper-content step=\"1\">\n                        <v-card\n                        class=\"mb-12 pt-0\"\n                        \n                        min-height=\"fill-height\"\n                        style=\"min-height : 100px; \"\n                        >\n                            <v-row class=\"px-10 py-10\">\n                                <v-col\n                                    cols=\"12\"\n                                    sm=\"12\"\n                                    md=\"12\"\n                                    lg=\"12\"\n                                >\n                                    <validation-provider \n                                        v-slot=\"{errors}\"\n                                        name=\"Owner\"\n                                        rules=\"required\"\n                                    >\n                                        <v-text-field\n                                            v-model=\"ownerFullName\"\n                                            :error-messages=\"errors\"\n                                            label=\"Select Owner\"\n                                            readonly\n                                            required\n                                        ></v-text-field>\n                                    </validation-provider>\n                                    <validation-provider \n                                        v-slot=\"{errors}\"\n                                        name=\"Property name\"\n                                        rules=\"required\"\n                                    >\n                                        <v-text-field\n                                            v-model=\"property.name\"\n                                            :error-messages=\"errors\"\n                                            label=\"Property Name\"\n                                        ></v-text-field>\n                                    </validation-provider>\n                                    <validation-provider\n                                        v-slot=\"{errors}\"\n                                        name=\"Type\"\n                                        rules=\"required\"\n                                    >\n                                        <v-select\n                                            v-model=\"property.type\"\n                                            :error-messages=\"errors\"\n                                            :items=\"items\"\n                                            label=\"Property Type\"\n                                            required\n                                            @change=\"$v.select.$touch()\"\n                                            @blur=\"$v.select.$touch()\"\n                                        ></v-select>\n                                    </validation-provider>\n                                    <validation-provider \n                                        v-slot=\"{errors}\"\n                                        name=\"Total units\"\n                                        rules=\"required\"\n                                    >\n                                        <v-text-field\n                                            v-model=\"property.units\"\n                                            :error-messages=\"errors\"\n                                            label=\"Total unit\"\n                                        ></v-text-field>\n                                    </validation-provider>\n                                    <validation-provider\n                                        v-slot=\"{errors}\"\n                                        name=\"Size\"\n                                    >\n                                        <v-text-field\n                                            v-model=\"property.size\"\n                                            :error-messages=\"errors\"\n                                            label=\"Size (in Sq. ft)\"\n                                            required\n                                            @input=\"$v.email.$touch()\"\n                                            @blur=\"$v.email.$touch()\"\n                                        ></v-text-field>\n                                    </validation-provider>\n                                    <validation-provider\n                                        v-slot=\"{errors}\"\n                                        name=\"Bocks\"\n                                    >\n                                        <v-text-field\n                                            v-model=\"property.blocks\"\n                                            :error-messages=\"errors\"\n                                            label=\"No. of Blocks\"\n                                            required\n                                            @input=\"$v.email.$touch()\"\n                                            @blur=\"$v.email.$touch()\"\n                                        ></v-text-field>\n                                    </validation-provider>\n                                </v-col>\n                            </v-row>\n                        </v-card>\n\n                        <v-btn\n                        color=\"success\"\n                        @click=\"saveStepOne\"\n                        >\n                        Continue\n                        </v-btn>\n\n                        <v-btn text>\n                        Cancel\n                        </v-btn>\n                    </v-stepper-content>\n\n                    <v-stepper-content step=\"2\">\n                        <v-card\n                        class=\"mb-12\"\n                        height=\"100%\"\n                        >\n                            <div  class=\"px-10 py-10\">\n                                <v-text-field\n                                    v-model=\"property.address\"\n                                    :error-messages=\"nameErrors\"\n                                    label=\"Address\"\n                                    required\n                                    @input=\"$v.name.$touch()\"\n                                    @blur=\"$v.name.$touch()\"\n                                ></v-text-field>\n                                <v-row> \n                                    <v-col>\n                                        <validation-provider\n                                            v-slot=\"{errors}\"\n                                            name=\"City/District\"\n                                        >\n                                            <v-text-field\n                                                v-model=\"property.city\"\n                                                :error-messages=\"errors\"\n                                                label=\"City/District\"\n                                                required\n                                                @input=\"$v.name.$touch()\"\n                                                @blur=\"$v.name.$touch()\"\n                                            ></v-text-field>\n                                        </validation-provider>\n                                    </v-col>\n                                    <v-col>\n                                        <validation-provider\n                                            v-slot=\"{errors}\"\n                                            name=\"County/State/Province\"\n                                        >\n                                            <v-text-field\n                                                v-model=\"property.state\"\n                                                :error-messages=\"errors\"\n                                                label=\"County/State/Province\"\n                                                required\n                                                @input=\"$v.name.$touch()\"\n                                                @blur=\"$v.name.$touch()\"\n                                            ></v-text-field>\n                                        </validation-provider>\n                                    </v-col> \n                                </v-row> \n                                <v-row> \n                                    <v-col>\n                                        <validation-provider\n                                            v-slot=\"{errors}\"\n                                            name=\"Postal Code\"\n                                        >\n                                            <v-text-field\n                                                v-model=\"property.code\"\n                                                :error-messages=\"errors\"\n                                                label=\"Postal Code\"\n                                                required\n                                                @input=\"$v.name.$touch()\"\n                                                @blur=\"$v.name.$touch()\"\n                                            ></v-text-field>\n                                        </validation-provider>\n                                    </v-col>\n                                    <v-col>\n                                        <validation-provider\n                                            v-slot=\"{errors}\"\n                                            name=\"Country\"\n                                        >\n                                            <v-text-field\n                                                v-model=\"property.country\"\n                                                :error-messages=\"errors\"\n                                                label=\"Country\"\n                                                required\n                                                @input=\"$v.name.$touch()\"\n                                                @blur=\"$v.name.$touch()\"\n                                            ></v-text-field>\n                                        </validation-provider>\n                                    </v-col> \n                                </v-row>\n                            </div>\n                        </v-card>\n\n                        <v-btn\n                        color=\"success\"\n                        @click=\"saveStepTwo\"\n                        >\n                        Continue\n                        </v-btn>\n\n                        <v-btn text>\n                        Cancel\n                        </v-btn>\n                    </v-stepper-content>\n\n                    <v-stepper-content step=\"3\">\n                        <v-card\n                        class=\"mb-12\"\n                        height=\"100%\"\n                        >\n                            <div class=\"py-10 px-10\">\n                                <!-- <v-row> \n                                    <v-col> -->\n                                        <validation-provider\n                                            v-slot=\"{errors}\"\n                                            name=\"Manager\"\n                                        >\n                                            <v-text-field\n                                                v-model=\"name\"\n                                                :error-messages=\"errors\"\n                                                label=\"Manager\"\n                                                required\n                                                @input=\"$v.name.$touch()\"\n                                                @blur=\"$v.name.$touch()\"\n                                            ></v-text-field>\n                                        </validation-provider>\n                                    <!-- </v-col>\n                                    <v-col> -->\n                                        <validation-provider\n                                            v-slot=\"{errors}\"\n                                            name=\"Location/Area\"\n                                        >\n                                            <v-text-field\n                                                v-model=\"name\"\n                                                :error-messages=\"errors\"\n                                                label=\"Location/Area\"\n                                                required\n                                                @input=\"$v.name.$touch()\"\n                                                @blur=\"$v.name.$touch()\"\n                                            ></v-text-field>\n                                        </validation-provider>\n                                    <!-- </v-col>\n                                </v-row> -->\n                            </div> \n                        </v-card>\n\n                        <v-btn\n                        color=\"success text-capitalize\"\n                        @click=\"saveProperty\"\n                        >\n                        Save\n                        </v-btn>\n\n                        <v-btn text>\n                        Cancel\n                        </v-btn>\n                    </v-stepper-content>\n                    </v-stepper-items>\n                </v-stepper>\n\n\n\n\n            </v-col>     \n        </v-row> \n                <v-row v-if=\"hasProperty && showProperty\"> \n            <v-col lg=\"12\" class=\"mt-0\">\n                <v-card justify=\"start\" class=\"px-10\">\n                    <v-row>\n                        <v-col>\n                            <v-list-item two-line>\n                                <v-list-item-content>\n                                    <v-list-item-subtitle>Property Details</v-list-item-subtitle>\n                                </v-list-item-content>\n                            </v-list-item>\n                        </v-col>\n                        <!-- <v-col>\n                            <v-card-title>\n                                <v-text-field\n                                    v-model=\"search\"\n                                    append-icon=\"mdi-magnify\"\n                                    label=\"Search\"\n                                    single-line\n                                    hide-details\n                                ></v-text-field>\n                            </v-card-title>\n                        </v-col> -->\n                        <v-col class=\"mx-5\">\n                            <v-row>\n                                <v-spacer></v-spacer>\n                                <v-btn class=\"mx-2\">\n                                    <v-tooltip bottom>\n                                        <template v-slot:activator=\"{ on, attrs }\">\n                                            <v-icon\n                                            color=\"lime\"\n                                            dark\n                                            v-bind=\"attrs\"\n                                            v-on=\"on\"\n                                            >\n                                            mdi-book-open-variant\n                                            </v-icon>\n                                        </template>\n                                        <span>Items per page</span>\n                                    </v-tooltip>\n                                </v-btn>\n                                <v-btn class=\"mx-2\">\n                                    <v-tooltip bottom>\n                                        <template v-slot:activator=\"{ on, attrs }\">\n                                            <v-icon\n                                            color=\"green\"\n                                            dark\n                                            v-bind=\"attrs\"\n                                            v-on=\"on\"\n                                            @click=\"toggleAddProperty\"\n                                            >\n                                            mdi-plus-circle\n                                            </v-icon>\n                                        </template>\n                                        <span>Add Property</span>\n                                    </v-tooltip>\n                                </v-btn>\n                                <v-btn class=\"mx-2\">\n                                    <v-tooltip bottom>\n                                        <template v-slot:activator=\"{ on, attrs }\">\n                                            <v-icon\n                                            color=\"light-blue\"\n                                            dark\n                                            v-bind=\"attrs\"\n                                            v-on=\"on\"\n                                            >\n                                            mdi-clipboard-list\n                                            </v-icon>\n                                        </template>\n                                        <span>List Properties</span>\n                                    </v-tooltip>\n                                </v-btn>\n                                <v-btn class=\"mx-2\">\n                                    <v-tooltip bottom>\n                                        <template v-slot:activator=\"{ on, attrs }\">\n                                            <v-icon\n                                            color=\"primary\"\n                                            dark\n                                            v-bind=\"attrs\"\n                                            v-on=\"on\"\n                                            >\n                                            mdi-upload\n                                            </v-icon>\n                                        </template>\n                                        <span>Export</span>\n                                    </v-tooltip>\n                                </v-btn>\n                            </v-row>\n                        </v-col>\n                    </v-row>                \n                    <v-divider></v-divider>\n                    <div>\n                        <v-data-table\n                            style=\"height : 100%;\"\n                            item-key=\"name\"\n                            height=\"500\"\n                            :headers=\"headers\"\n                            :items=\"propertyList\"\n                            :page.sync=\"page\"\n                            :items-per-page=\"itemsPerPage\"\n                            hide-default-footer\n                            class=\"elevation-1\"\n                            @page-count=\"pageCount = $event\"                            \n                        >\n                            <template v-slot:no-data>\n                                <div style=\"margin-top: 15%; margin-bottom: 15%;\">\n                                    <!-- <v-alert :value=\"true\" color=\"error\" icon=\"warning\">\n                                        Sorry, nothing to display here :(\n                                    </v-alert> -->\n                                    <v-row justify=\"center\">\n                                        <v-btn\n                                            color=\"success text-capitalize\"\n                                            rounded\n                                            @click=\"toggleAddProperty\"                \n                                        >\n                                            <v-icon>mdi-plus-circle</v-icon> Add Property\n                                        </v-btn>   \n                                    </v-row>\n                                </div>\n                            </template>\n                            <template v-slot:item.actions=\"{ item }\">\n                                <v-icon\n                                    medium\n                                    class=\"mr-2 blue--text\"\n                                    @click=\"editItem(item)\"\n                                >\n                                    mdi-pencil-circle\n                                </v-icon>\n                                <v-icon\n                                    medium\n                                    class=\"red--text\"\n                                    @click=\"deleteItem(item)\"\n                                >\n                                    mdi-delete\n                                </v-icon>\n                            </template>\n                        </v-data-table>                        \n                        <div class=\"text-center pt-2\">\n                        <v-pagination\n                            v-model=\"page\"\n                            :length=\"pageCount\"\n                            class=\"py-10\"\n                        ></v-pagination>\n                        <!-- <v-text-field\n                            :value=\"itemsPerPage\"\n                            label=\"Items per page\"\n                            type=\"number\"\n                            min=\"-1\"\n                            max=\"15\"\n                            @input=\"itemsPerPage = parseInt($event, 10)\"\n                        ></v-text-field> -->\n                        </div>\n                    </div>\n                </v-card>\n            </v-col>\n        </v-row>      \n    </v-container>\n</template>\n<script>\nimport Dialog from '../components/Dialog.vue';\nimport PropertyModel from '../models/PropertyModel';\nexport default {\n    component : {Dialog},\n    name : 'property',\n    data() {\n        return { \n            property : new PropertyModel('','','','','','','','','','','','',''),\n            no_data : true,\n            e1 : 1,\n            hasProperty : true,\n            addProperty : false,\n            showProperty : true,\n            page: 1,\n            pageCount: 0,\n            itemsPerPage: 10,\n            headers: [\n                {\n                    text: 'Property',\n                    align: 'start',\n                    sortable: false,\n                    value: 'attributes.name',\n                },\n                { text: 'Units', value: 'attributes.units' },\n                { text: 'Type', value: 'fat' },\n                { text: 'Size', value: 'carbs' },\n                { text: 'Blocks', value: 'protein' },\n                { \n                    text: 'Manager', \n                    align : 'start',\n                    value: 'relationships.caretakers.data.relationships' \n                },\n                { text: 'Actions', value: 'actions', sortable: false},\n            ],\n            propertyList : [],\n        }\n    },\n    mounted()  {\n        console.log(\"this page can refresh\")\n        this.$nextTick(() => this.getPropertyList())\n    },\n    computed : {\n        ownerID () {\n            return this.$store.state.auth.profile.relationships.landlord.data.id ?? null\n        },\n        ownerFullName() {\n            let data = this.$store.state.auth.profile.attributes\n            let name = data.firstName + \" \" + data.lastName\n            return name ?? null\n        }\n    },\n    methods:{\n        toggleAddProperty(){\n            this.showProperty = false;\n            this.addProperty = true;\n        },\n        saveStepOne() {\n            this.property.owner_id = this.ownerID\n            console.log(this.property)\n            this.e1 = 2\n        },\n        saveStepTwo() {\n            console.log(this.property)\n            this.e1 = 3\n        },\n        saveStepThree() {\n\n        },\n        saveProperty() {\n            this.$store.dispatch('propertyDetails/createNewProperty', this.property).then(\n                success => {\n                    let data = success.data\n                    if(data) {\n                        this.addProperty = false;\n                        this.hasProperty = true;            \n                        this.showProperty = true;\n                    }\n                },\n                error => {\n                    if(error) {\n                        console.log(error)\n                    }\n                }\n            );            \n        },\n        getPropertyList() {\n            this.$store.dispatch('propertyDetails/propertyList', this.ownerID).then(\n                list => {\n                    let data = list.data\n                    console.log(data)\n                    if(data.length >0) {\n                        this.propertyList = data\n                        this.addProperty = false\n                        this.hasProperty = true                  \n                        this.showProperty = true;\n                    } \n                },\n                error => {\n                    console.log(error)\n                }\n            );\n        },\n        editItem(item) {\n            alert(item.id)\n        }\n    }\n}\n</script>"]}]}