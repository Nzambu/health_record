{"remainingRequest":"/var/www/html/app/node_modules/vue-loader/lib/index.js??vue-loader-options!/var/www/html/app/src/views/Login.vue?vue&type=script&lang=js&","dependencies":[{"path":"/var/www/html/app/src/views/Login.vue","mtime":1605880438892},{"path":"/var/www/html/app/node_modules/cache-loader/dist/cjs.js","mtime":1605767621944},{"path":"/var/www/html/app/node_modules/babel-loader/lib/index.js","mtime":1605767621946},{"path":"/var/www/html/app/node_modules/cache-loader/dist/cjs.js","mtime":1605767621944},{"path":"/var/www/html/app/node_modules/vue-loader/lib/index.js","mtime":1605767635826}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Login.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Login.vue","sourceRoot":"src/views","sourcesContent":["<template>\n    <v-container> \n        <v-row>\n            <v-col \n                offset-sm=\"1\"\n                sm=\"10\"\n                offset-md=\"2\"\n                md=\"8\"\n                offset-lg=\"4\"\n                lg=\"4\"                \n            >\n                <v-card class=\"px-10 py-10 rounded-lg\">\n                    <v-card-text class=\"pb-10\">\n                        <h1 class=\"text-center primary--text\">Sign in to Vuex</h1>\n                    </v-card-text>\n                    <v-form\n                        ref=\"form\"\n                        v-model=\"valid\"\n                        lazy-validation\n                    >\n                        <v-col>\n                            <v-alert v-if=\"message\" dense type=\"error\">{{message}}</v-alert>\n                        </v-col>\n                        <v-text-field\n                        v-model=\"user.userName\"\n                        :rules=\"nameRules\"\n                        label=\"Name\"\n                        required\n                        ></v-text-field>\n\n                        <v-text-field\n                        v-model=\"user.userPassword\"\n                        :rules=\"passwordRules\"\n                        label=\"Password\"\n                        required\n                        ></v-text-field>\n\n                        <v-text-field\n                        v-model=\"user.companyCode\"\n                        :rules=\"codeRules\"\n                        label=\"Company code\"\n                        required\n                        ></v-text-field>\n\n                        <v-col class=\"text-right pr-0 pt-0 pb-0\">\n                            <v-btn\n                                :loading=\"loading\"\n                                :disabled=\"!valid\"\n                                color=\"success\"\n                                class=\"mr-0\"\n                                @click=\"handleLogin\"\n                            >\n                            LOGIN\n                            <!-- <v-progress-circular\n                                indeterminate\n                                color=\"purple\"\n                            ></v-progress-circular> -->\n                            </v-btn>\n                        </v-col>\n                    </v-form>\n                </v-card>\n                <v-col>\n                    <p class=\"text-center\"><small>Copyright &copy; 2020 <span class=\"primary--text\">Vue and Vuex</span></small></p>\n                </v-col>\n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n\n<script>\nimport User from '../models/User'\nexport default {\n    name : 'Login',\n    data () {\n        return {\n            user : new User('', '', ''),\n            valid : true,\n            loading : false,\n            message : '',\n            name : '',\n            nameRules : [\n                v => !!v || 'Name is required',\n                v => (v && v.length <= 10) || 'Name must be less than 10 characters',\n            ],\n            email : '',\n            passwordRules : [\n                v => !!v || 'Password is required',\n                v => (v && v.length >= 8) || 'Password must be equal to or longer that 8 characters',\n            ],\n            codeRules : [\n                v => !!v || 'Company code is required',\n                v => (v && v.length <= 3) || 'Name must be more than 3 characters',\n            ],\n        }\n    },\n    computed: {\n        loggedIn() {\n            return this.$store.state.auth.status.loggedIn;\n            }\n        },\n    methods : {\n        handleLogin() {\n            this.loading = !this.loading;\n            console.log(this.loading)\n            this.$validator.validateAll().then(isValid => {\n                if (!isValid) {\n                    this.loading = false;\n                    console.log(\"This is not working at all\")\n                    return;\n                }\n\n                if (this.user.userName && this.user.userPassword && this.user.companyCode) {\n                    this.$store.dispatch('auth/login', this.user).then(\n                        () => {\n                            // this.$router.push('/profile');\n                            // this.$router.replace(this.$route.query.from)\n                            this.$router.push(this.$route.query.from || '/about') \n                        },\n                        error => {\n                        console.log(error.response)\n                        this.loading = false;\n                        this.message =\n                            (error.response && error.response.data.error.message) ||\n                            error.message ||\n                            error.toString();\n                        }\n                    )\n                }\n            });\n        }\n    }\n}\n</script>"]}]}