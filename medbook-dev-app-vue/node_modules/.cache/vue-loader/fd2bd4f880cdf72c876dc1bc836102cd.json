r-messages=\"errors\"\n                                                    label=\"Country\"\n                                                    required\n                                                    @input=\"$v.name.$touch()\"\n                                                    @blur=\"$v.name.$touch()\"\n                                                ></v-text-field>\n                                            </validation-provider>\n                                        </v-col> \n                                    </v-row>\n                                </div>\n                            </v-card>\n                        <!-- Continue and cancel buttons -->\n                        <v-btn \n                            text\n                            class=\"mr-2 text-capitalize\"\n                            @click=\"cancelAddProperty\"\n                        >\n                            Cancel\n                        </v-btn>\n                        <v-btn\n                            color=\"success\"\n                            class=\"text-capitalize\"\n                            @click=\"saveStepTwo\"\n                            :disabled=\"invalid\"\n                        >\n                            Continue\n                        </v-btn>\n                        <!--  -->\n                        </validation-observer>\n                    </v-stepper-content>\n\n                    <v-stepper-content step=\"3\">\n                        <validation-observer\n                            ref=\"stepThreeObserver\"\n                            v-slot=\"{invalid}\"\n                        >\n                            <v-card\n                                class=\"mb-12\"\n                                height=\"100%\"\n                            >                            \n                                <div class=\"py-10 px-10\">\n                                    <!-- <v-row> \n                              