ntent step=\"1\">\n                        <validation-observer\n                            ref=\"stepOneObserver\"\n                            v-slot=\"{invalid}\"\n                        >\n                            <v-card\n                                class=\"mb-12 pt-0\"                        \n                                min-height=\"fill-height\"\n                                style=\"min-height : 100px; \"\n                            >\n                                <v-row class=\"px-10 py-10\">\n                                    <v-col\n                                        cols=\"12\"\n                                        sm=\"12\"\n                                        md=\"12\"\n                                        lg=\"12\"\n                                    >    \n                                        <v-form ref=\"stepOneForm\" v-model=\"stepOneFormValidation\" lazy-validation>\n                                            <validation-provider \n                                                v-slot=\"{errors}\"\n                                                name=\"Owner\"\n                                                rules=\"required\"\n                                            >\n                                                <v-text-field\n                                                    v-model=\"ownerFullName\"\n                                                    :error-messages=\"errors\"\n                                                    label=\"Select Owner\"\n                                                    readonly\n                                                    required\n                                                ></v-text-field>\n                                            </validation-provider>\n                                            <validation-provider \n                                                v-slot=\"{errors}\"\n                                                name=\"Property name\"\n                                                rules=\"required\"\n                                            >\n                                                <v-text-field\n                                                    name=\"name\"\n                                                    v-model=\"property.name\"\n                                                    :rules=\"required\"\n                                                    label=\"Property Name\"\n                                                    :error-messages=\"errors\"                                                \n                                                ></v-text-field>\n                                            </validation-provider>\n                                            <validation-provider\n                                                v-slot=\"{errors}\"\n                                                name=\"Type\"\n                                                rules=\"required\"                                                \n                                            >\n                                                <v-select\n                                                    v-model=\"property.type\"\n                                                    :error-messages=\"errors\"\n                                                    :items=\"items\"\n                                                    label=\"Property Type\"\n                                                    @change=\"$v.select.$touch()\"\n                                                    @blur=\"$v.select.$touch()\"\n                                                ></v-select>\n                                            </validation-provider>\n                                            <validation-provider \n                                                v-slot=\"{errors}\"\n                                                name=\"Total units\"\n                