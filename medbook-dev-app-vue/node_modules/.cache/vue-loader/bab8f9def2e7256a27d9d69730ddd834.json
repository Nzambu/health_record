trong>Company:</strong>\n      {{company}}\n    </p>\n    <p>\n      <strong>Companies:</strong>\n      {{companies[0]}}\n    </p>\n    <p>\n      <strong>Id:</strong>\n      {{user.id}}\n    </p>\n    <p>\n      <strong>Email:</strong>\n      {{user.attribute.email}}\n    </p>\n    <strong>attributes:</strong>\n    <ul>\n      <li v-for=\"(role,index) in user.attribute\" :key=\"index\">{{index}} => {{role}}</li>\n    </ul>\n  </div>\n\n</template>\n\n<script>\nexport default {\n  name: 'Profile',\n  computed: {\n    user() {\n      return this.$store.state.auth.user;\n    },\n    language() {\n      return this.$store.state.userDetails.language\n    },\n    languages() {\n      return this.$store.state.userDetails.languages\n    },\n    company() {\n      return this.$store.state.userDetails.company\n    },\n    companies() {\n      return this.$store.state.userDetails.companies\n    }\n  },\n  created() {    \n    this.$nextTick(() => {\n      this.getLanguage()   \n      this.getLanguages() \n      this.getCompany() \n      this.getCompanies()\n    })\n  },\n  mounted() {\n    if (!this.user) {\n      this.$router.push('/login');\n    } \n  },\n  methods: {\n    getLanguage() {\n      this.$store.dispatch('userDetails/language', this.user).then(\n        (language) => {\n          return language.data\n        },\n        error => {\n          return error.response\n        }\n      )\n    },\n    getLanguages() {\n      this.$store.dispatch('userDetails/languages', this.user).then(\n        (languages) => {\n          return languages.data.data\n        },\n        error => {\n          return error\n        }\n      )\n    },\n    getCompany() {\n      this.$store.dispatch('userDetails/company', this.user).then(\n        (company) => {\n          return company.data\n        },\n        error => {\n          return error\n        }\n      )\n    },\n    getCompanies() {\n      this.$store.dispatch('userDetails/companies', this.user).then(\n        (companies) => {\n          return companies.data\n        },\n        error => {\n          return error\n        }\n      )\n    }\n  }\n};\n</script>\n"]}]}