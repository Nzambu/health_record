v-radio value=\"Google\">\n                            <template v-slot:label>\n                            <div><strong class=\"success--text\">M-pesa pay bill</strong></div>\n                            </template>\n                        </v-radio>\n                        <v-radio value=\"Duckduckgo\">\n                            <template v-slot:label>\n                            <div> <strong class=\"default--text\">Bank deposit</strong></div>\n                            </template>\n                        </v-radio>\n                    </v-radio-group>\n                </v-card>\n            </v-col>\n            <v-col\n                cols=\"12\"\n                sm=\"12\"\n                md=\"6\"\n                lg=\"6\"\n            >\n                <v-card \n                    class=\"px-10 py-10\"\n                >\n                    <v-text-field\n                        v-model=\"email\"\n                        :error-messages=\"emailErrors\"\n                        label=\"Rent Amount\"\n                        required\n                        @input=\"$v.email.$touch()\"\n                        @blur=\"$v.email.$touch()\"\n                    ></v-text-field>\n                    <v-text-field\n                        v-model=\"email\"\n                        :error-messages=\"emailErrors\"\n                        label=\"Overdue Amount\"\n                        required\n                        @input=\"$v.email.$touch()\"\n                        @blur=\"$v.email.$touch()\"\n                    ></v-text-field>\n                    <v-text-field\n                        v-model=\"email\"\n                        :error-messages=\"emailErrors\"\n                        label=\"Amount to pay\"\n                        required\n                        @input=\"$v.email.$touch()\"\n                        @blur=\"$v.email.$touch()\"\n                    ></v-text-field>\n                    <v-text-field\n                        