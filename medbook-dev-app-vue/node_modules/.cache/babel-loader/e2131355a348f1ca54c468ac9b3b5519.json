   state: initialState,\n    actions: {\n        user({ commit }, user) {\n            return UserService.getUser(user.id).then(\n                success => {\n                    commit('userSuccess', success.data.data);\n                    return Promise.resolve(success);\n                },\n                error => {\n                    commit('userFail');\n                    return Promise.reject(error);\n                }\n            );\n        },\n        tenant({ commit }, user) {\n            return UserService.getTenant(user.relationships.tenant.id).then(\n                success => {\n                    commit('tenantSuccess', success.data.data);\n                    return Promise.resolve(success);\n                },\n                error => {\n                    commit('tenantFail');\n                    return Promise.reject(error);\n                }\n            );\n        },\n        owner({ commit }, user) {\n            return UserService.getOwner(user.relationships.owner.id).then(\n                success => {\n                    commit('ownerSuccess', success.data.data);\n                    return Promise.resolve(success);\n                },\n                error => {\n                    commit('ownerFail');\n                    return Promise.reject(error);\n                }\n            );\n        },\n        manager({ commit }, user) {\n            return UserService.getManager(user.relationships.manager.id).then(\n                success => {\n                    commit('managerSuccess', success.data.data);\n                    return Promise.resolve(success);\n                },\n                error => {\n                    commit('managerFail');\n                    return Promise.reject(error);\n                }\n            );\n        },\n    },\n    mutations: {\n        userSuccess(state, success) {\n            state.user = success;\n        },\n        userFail(state, error) {\n            state.user['error'] = error;\n        },\n        tenantSuccess(state, success) {\n            state.tenant = success;\n        },\n        tenantFail(state, error) {\n            state.tenant['error'] = error;\n        },\n        managerSuccess(state, success) {\n            state.manager = success;\n        },\n        managerFail(state, error) {\n            state.manager['error'] = error;\n        },\n    }\n}\n"]}]}