     commit('companySuccess', company.data.data);\n                    return Promise.resolve(company);\n                },\n                error => {\n                    commit('companyFailure');\n                    return Promise.resolve(error);\n                }\n            )\n        },\n        companies({ commit }, user) {\n            return UserService.companies(user.id).then(\n                companies => {\n                    commit('companiesSuccess', companies.data.data);\n                    return Promise.resolve(companies);\n                },\n                error => {\n                    commit('companiesFailure');\n                    return Promise.resolve(error);\n                }\n            )\n        },\n        events({commit}, user) {\n            return UserService.events(user.id).then(\n                events => {\n                    console.log(events)\n                    commit('eventsSuccess', events.data.data)\n                    return Promise.resolve(events)\n                },\n                error => {\n                    commit('eventsFailure');\n                    return Promise.resolve(error);\n                }\n            )\n        }\n    },\n    mutations: {\n        languageSuccess(state, language) {\n            state.language = language;\n        },\n        languageFailure(state) {\n            state.language = null;\n        },\n        languagesSuccess(state, languages) {\n            state.languages = languages;\n        },\n        languagesFailure(state) {\n            state.languages = null;\n        },\n        companySuccess(state, company) {\n            state.company = company;\n        },\n        companyFailure(state) {\n            state.company = null;\n        },\n        companiesSuccess(state, companies) {\n            state.companies = companies;\n        },\n        companiesFailure(state) {\n            state.companies = null;\n        },\n        eventsSuccess(state, events) {\n            state.events = events;\n        },\n        eventsFailure(state) {\n            state.events = null;\n        }\n    }\n}\n"]}]}