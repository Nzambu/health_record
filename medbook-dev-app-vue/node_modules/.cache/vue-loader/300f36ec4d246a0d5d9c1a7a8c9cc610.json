                     :rules=\"nameRules\"\n                        label=\"Name\"\n                        required\n                        ></v-text-field>\n\n                        <v-text-field\n                        v-model=\"user.userPassword\"\n                        :rules=\"passwordRules\"\n                        label=\"Password\"\n                        required\n                        ></v-text-field>\n\n                        <v-text-field\n                        v-model=\"user.companyCode\"\n                        :rules=\"codeRules\"\n                        label=\"Company code\"\n                        required\n                        ></v-text-field>\n\n                        <v-col class=\"text-right pr-0 pt-0 pb-0\">\n                            <v-btn\n                                :disabled=\"!valid\"\n                                color=\"success\"\n                                class=\"mr-0\"\n                                @click=\"handleLogin\"\n                            >\n                            LOGIN\n                            </v-btn>\n                        </v-col>\n                    </v-form>\n                </v-card>\n                <v-col>\n                    <p class=\"text-center\"><small>Copyright &copy; 2020 <span class=\"primary--text\">Vue and Vuex</span></small></p>\n                </v-col>\n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n\n<script>\nimport AuthService from '../services/AuthService.js'\nimport User from '../models/User'\nexport default {\n    name : 'Login',\n    data () {\n        return {\n            user : new User('', '', ''),\n            authService : AuthService,\n            valid: true,\n            name: '',\n            nameRules: [\n                v => !!v || 'Name is required',\n                v => (v && v.length <= 10) || 'Name must be less than 10 characters',\n            ],\n            email: '',\n            passwordRules: [\n                v => !!v || 'Password is required',\n                v => (v && v.length >= 8) || 'Password must be equal to or longer that 8 characters',\n            ],\n            codeRules: [\n                v => !!v || 'Company code is required',\n                v => (v && v.length <= 3) || 'Name must be more than 3 characters',\n            ],\n        }\n    },\n    computed: {\n        loggedIn() {\n            return this.$store.state.auth.status.loggedIn;\n            }\n        },\n    methods : {\n        signIn(){\n            // let data = {\n            //     // \"companyCode\" : \"dev\",\n            //     \"userName\" : \"Nzioki\",\n            //     \"userPassword\" : \"password\"\n            // }\n            // var feedback = this.authService.login(data)\n            // console.log(feedback.status)\n            console.log(this.user.userName, this.user.userPassword, this.user.companyCode)\n        },\n        handleLogin() {\n            this.loading = true;\n            this.$validator.validateAll().then(isValid => {\n                if (!isValid) {\n                    this.loading = false;\n                    console.log(\"This is not working at all\")\n                    return;\n                }\n\n                if (this.user.userName && this.user.userPassword && this.user.companyCode) {\n                    this.$store.dispatch('auth/login', this.user).then(\n                        () => {\n                            // console.log(\"credentioals\")\n                            this.$router.push('/about');\n                        },\n                        error => {\n                            console.log(error)\n                        this.loading = false;\n                        this.message =\n                            (error.response && error.response.data) ||\n                            error.message ||\n                            error.toString();\n                        }\n                    )\n                }\n            });\n        }\n    }\n}\n</script>"]}]}