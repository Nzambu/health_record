AAA,CADA,CAEA;AACA;AACA;AACA;AACA;AACA;;AAEA,UAAA,KAAA,IAAA,CAAA,KAAA,IAAA,KAAA,IAAA,CAAA,QAAA,EAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,YAAA,EAAA,KAAA,IAAA,EAAA,IAAA,CACA,YAAA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,IAAA,YAAA;AACA,SAHA,EAIA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,OAAA,GACA,KAAA,CAAA,QAAA,IAAA,KAAA,CAAA,QAAA,IACA,KAAA,CAAA,OADA,IAEA,KAAA,CAAA,QAAA,EAHA;AAIA,SAXA;AAaA,OAvBA,CAwBA;;AACA;AA1BA;AA9BA,CAAA","sourcesContent":["<template>\n    <v-img \n        :src=\"backgroundImg\"\n        max-width=\"auto\"\n        max-height=\"auto\"\n    >\n        <v-container \n            fluid \n            fill-height\n        >\n            <v-row >\n                <v-col \n                    offset-sm=\"1\"\n                    sm=\"10\"\n                    offset-md=\"4\"\n                    md=\"6\"\n                    offset-lg=\"7\"\n                    lg=\"3\"              \n                >\n                    <v-card class=\"px-10 pt-10 pb-3 rounded-lg\">\n                        <v-card-text class=\" mt-0 pb-10\">\n                            <h1 class=\"text-center primary--text\">Sign in to Vuex</h1>\n                        </v-card-text>\n                        <v-form\n                            ref=\"form\"\n                            v-model=\"valid\"\n                            lazy-validation\n                        >\n                            <v-col>\n                                <v-alert v-if=\"message\" dense type=\"error\">{{message}}</v-alert>\n                            </v-col>\n                            <v-text-field\n                            v-model=\"user.email\"\n                            :rules=\"nameRules\"\n                            label=\"Name\"\n                            required\n                            ></v-text-field>\n\n                            <v-text-field\n                            v-model=\"user.password\"\n                            :rules=\"passwordRules\"\n                            label=\"Password\"\n                            type=\"password\"\n                            required\n                            ></v-text-field>\n\n                            <!-- <v-text-field\n                            v-model=\"user.companyCode\"\n                            :rules=\"codeRules\"\n                            label=\"Company code\"\n                            required\n                            ></v-text-field> -->\n\n                            <v-col class=\"text-right pr-0 pt-0 pb-0\">\n                                <v-btn\n                                    :loading=\"loading\"\n                                    :disabled=\"!valid\"\n                                    color=\"success\"\n                                    class=\"mr-0\"\n                                    @click=\"handleLogin\"\n                                >\n                                LOGIN\n                                <!-- <v-progress-circular\n                                    indeterminate\n                                    color=\"purple\"\n                                ></v-progress-circular> -->\n                                </v-btn>\n                            </v-col>\n                        </v-form>\n                        <v-col class=\"pl-0 pt-0\">\n                        <p class=\"text-left py-0 pl-0\"><small>Forgot password? <span class=\"primary--text\">Click here</span></small></p>\n                    </v-col>  \n                    <v-col class=\"py-0\">\n                        <p class=\"text-center py-0 px-0\"><small>Copyright &copy; 2020 <span class=\"primary--text\">Vue and Vuex</span></small></p>\n                    </v-col>                      \n                    </v-card>  \n                    <!-- <v-col>\n                        <p class=\"text-center\"><small>Copyright &copy; 2020 <span class=\"primary--text\">Vue and Vuex</span></small></p>\n                    </v-col>                   -->\n                </v-col>\n            </v-row>\n            <!-- <v-row>\n                <v-col>\n                    <p class=\"text-center\"><small>Copyright &copy; 2020 <span class=\"primary--text\">Vue and Vuex</span></small></p>\n                </v-col>\n            </v-row> -->\n        </v-container>   \n    </v-img> \n</template>\n\n<script>\nimport User from '../models/User'\nimport backgroundImage from '../assets/property2.jpg'\nexport default {\n    name : 'Login',\n    data () {\n        return {\n            backgroundImg : backgroundImage,\n            user : new User('', '', ''),\n            valid : true,\n            loading : false,\n            message : '',\n            name : '',\n            nameRules : [\n                v => !!v || 'Name is required',\n                v => (v && v.length > 1) || 'Name must be less than 10 characters',\n            ],\n            email : '',\n            passwordRules : [\n                v => !!v || 'Password is required',\n                v => (v && v.length >= 8) || 'Password must be equal to or longer that 8 characters',\n            ],\n            codeRules : [\n                v => !!v || 'Company code is required',\n                v => (v && v.length <= 3) || 'Name must be more than 3 characters',\n            ],\n        }\n    },\n    computed: {\n        loggedIn() {\n            return this.$store.state.auth.status.loggedIn;\n            }\n        },\n    methods : {\n        handleLogin() {\n            this.loading = !this.loading;\n            // this.$validator.validateAll().then(isValid => {\n            //     if (!isValid) {\n            //         this.loading = false;\n            //         console.log(\"This is not working at all\")\n            //         return;\n            //     } \n\n                if (this.user.email && this.user.password) {                    \n                    this.$store.dispatch('auth/login', this.user).then(\n                        () => {\n                            this.$router.push(this.$route.query.from || '/dashboard') \n                        },\n                        error => {\n                        console.log(error)\n                        this.loading = false;\n                        this.message =\n                            (error.response && error.response) ||\n                            error.message ||\n                            error.toString();\n                        }\n                    )\n                }\n            // });\n        }\n    }\n}\n</script>"],"sourceRoot":"src/views"}]}