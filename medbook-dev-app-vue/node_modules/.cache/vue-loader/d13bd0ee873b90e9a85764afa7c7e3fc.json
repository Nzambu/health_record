vider\n                                v-slot=\"{errors}\"\n                                name=\"Property\"\n                                rules=\"required\"\n                            >\n                                <v-select\n                                    v-model=\"manager.property\"\n                                    :error-messages=\"errors\"\n                                    :items=\"items\"\n                                    label=\"Property\"\n                                    required\n                                    @change=\"$v.select.$touch()\"\n                                    @blur=\"$v.select.$touch()\"\n                                ></v-select>\n                            </validation-provider>\n                            <v-menu\n                                v-model=\"menu1\"\n                                :close-on-content-click=\"false\"\n                                max-width=\"290\"\n                            >\n                                <template v-slot:activator=\"{ on, attrs }\">\n                                    <validation-provider\n                                        v-slot=\"{errors}\"\n                                        name=\"Start Date\"\n                                        rules=\"required\"\n                                    >\n                                        <v-text-field\n                                            :value=\"computedDateFormattedMomentjs\"\n                                            :error-messages=\"errors\"\n                                            clearable\n                                            label=\"Starting Date\"\n                                            readonly\n                                            v-bind=\"attrs\"\n                                            v-on=\"on\"\n                                            @click:clear=\"date = null\"\n                                        ></v-text-field>\n        