            </v-row> -->\n                                </div> \n                            </v-card>\n                            <!-- Save and cancel buttons -->\n                            <v-btn \n                                text\n                                class=\"mr-2\"\n                            >\n                                Cancel\n                            </v-btn>\n                            <v-btn\n                                color=\"success text-capitalize\"\n                                @click=\"saveProperty\"\n                                :disabled=\"invalid\"\n                            >\n                                Save\n                            </v-btn>\n                            <!-- end of buttons -->\n                        </validation-observer>\n                    </v-stepper-content>\n                    </v-stepper-items>\n                </v-stepper>\n\n\n\n\n            </v-col>     \n        </v-row> \n                <v-row v-if=\"hasProperty && showProperty\"> \n            <v-col lg=\"12\" class=\"mt-0\">\n                <v-card justify=\"start\" class=\"px-10\">\n                    <v-row>\n                        <v-col>\n                            <v-list-item two-line>\n                                <v-list-item-content>\n                                    <v-list-item-subtitle>Property Details</v-list-item-subtitle>\n                                </v-list-item-content>\n                            </v-list-item>\n                        </v-col>\n                        <!-- <v-col>\n                            <v-card-title>\n                                <v-text-field\n                                    v-model=\"search\"\n                                    append-icon=\"mdi-magnify\"\n                                    label=\"Search\"\n                                    single-line\n                                    hide-details\n                                ></v-text-field>\n                            </v-card-title>\n                        </v-col> -->\n                        <v-col class=\"mx-5\">\n                            <v-row>\n                                <v-spacer></v-spacer>\n                                <v-btn class=\"mx-2\">\n                                    <v-tooltip bottom>\n                                        <template v-slot:activator=\"{ on, attrs }\">\n                                            <v-icon\n                                            color=\"lime\"\n                                            dark\n                                            v-bind=\"attrs\"\n                                            v-on=\"on\"\n                                            >\n                                            mdi-book-open-variant\n                                            </v-icon>\n                                        </template>\n                                        <span>Items per page</span>\n                                    </v-tooltip>\n                                </v-btn>\n                                <v-btn class=\"mx-2\">\n                                    <v-tooltip bottom>\n                                        <template v-slot:activator=\"{ on, attrs }\">\n                                            <v-icon\n                                            color=\"green\"\n                                            dark\n                                            v-bind=\"attrs\"\n                                            v-on=\"on\"\n                                            @click=\"toggleAddProperty\"\n                                            >\n                                            mdi-plus-circle\n                                            </v-icon>\n                                        </template>\n                                        <span>Add Property</span>\n                                    </v-tooltip>\n                                </v-btn>\n                                <v-btn class=\"mx-2\">\n                                    <v-tooltip bottom>\n                                        <template v-slot:activator=\"{ on, attrs }\">\n                                            <v-icon\n                                            color=\"light-blue\"\n                                            dark\n                                            v-bind=\"attrs\"\n                                            v-on=\"on\"\n                                            >\n                                            mdi-clipboard-list\n                                            </v-icon>\n                                        </template>\n                                        <span>List Properties</span>\n                                    </v-tooltip>\n                                </v-btn>\n                                <v-btn class=\"mx-2\">\n                                    <v-tooltip bottom>\n                                        <template v-slot:activator=\"{ on, attrs }\">\n                                            <v-icon\n                                            color=\"primary\"\n                                            dark\n                                            v-bind=\"attrs\"\n                                            v-on=\"on\"\n                                            >\n                                            mdi-upload\n                                            </v-icon>\n                                        </template>\n                                        <span>Export</span>\n                                    </v-tooltip>\n                                </v-btn>\n                            </v-row>\n                        </v-col>\n                    </v-row>                \n                    <v-divider></v-divider>\n                    <div>\n                        <v-data-table\n                            style=\"height : 100%;\"\n                            item-key=\"name\"\n                            height=\"500\"\n                            :headers=\"headers\"\n                            :items=\"propertyList\"\n                            :page.sync=\"page\"\n                            :items-per-page=\"itemsPerPage\"\n                            hide-default-footer\n                            class=\"elevation-1\"\n                            @page-count=\"pageCount = $event\"                            \n                        >\n                            <template v-slot:no-data>\n                                <div style=\"margin-top: 15%; margin-bottom: 15%;\">\n                                    <!-- <v-alert :value=\"true\" color=\"error\" icon=\"warning\">\n                                        Sorry, nothing to display here :(\n                                    </v-alert> -->\n                                    <v-row justify=\"center\">\n                                        <v-btn\n                                            color=\"success text-capitalize\"\n                                            rounded\n                                            @click=\"toggleAddProperty\"                \n                                        >\n                                            <v-icon>mdi-plus-circle</v-icon> Add Property\n                                        </v-btn>   \n                                    </v-row>\n                                </div>\n                            </template>\n                            <template v-slot:item.actions=\"{ item }\">\n                                <v-icon\n                                    medium\n                                    class=\"mr-2 blue--text\"\n                                    @click=\"editItem(item)\"\n                                >\n                                    mdi-pencil-circle\n                                </v-icon>\n                                <v-icon\n                                    medium\n                                    class=\"red--text\"\n                                    @click=\"deleteItem(item)\"\n                                >\n                                    mdi-delete\n                                </v-icon>\n                            </template>\n                        </v-data-table>                        \n                        <div class=\"text-center pt-2\">\n                        <v-pagination\n                            v-model=\"page\"\n                            :length=\"pageCount\"\n                            class=\"py-10\"\n                        ></v-pagination>\n                        <!-- <v-text-field\n                            :value=\"itemsPerPage\"\n                            label=\"Items per page\"\n                            type=\"number\"\n                            min=\"-1\"\n                            max=\"15\"\n                            @input=\"itemsPerPage = parseInt($event, 10)\"\n                        ></v-text-field> -->\n                        </div>\n                    </div>\n                </v-card>\n            </v-col>\n        </v-row> \n        <v-row v-if=\"showModal\">\n            <full-screen-modal></full-screen-modal>\n        </v-row>     \n    </v-container>\n</template>\n<script>\nimport Dialog from '../components/Dialog.vue';\nimport FullScreenModal from '../components/FullScreenModal.vue';\nimport PropertyModel from '../models/PropertyModel';\nexport default {\n  components: { FullScreenModal },\n    component : {Dialog},\n    name : 'property',\n    data() {\n        return { \n            property : new PropertyModel('','','','','','','','','','','','',''),\n            items : ['Residentials', 'Commercial', 'Mixed-use', 'Industrial', 'Retail'],\n            e1 : 1,\n            stepOneFormValidation : true,\n            hasProperty : true,\n            addProperty : false,\n            showProperty : true,\n            showModal : false,\n            page: 1,\n            pageCount: 0,\n            itemsPerPage: 10,\n            headers: [\n                {\n                    text: 'Property',\n                    align: 'start',\n                    sortable: false,\n                    value: 'attributes.name',\n                },\n                { text: 'Units', value: 'attributes.units' },\n                { text: 'Type', value: 'fat' },\n                { text: 'Size', value: 'carbs' },\n                { text: 'Blocks', value: 'protein' },\n                { \n                    text: 'Manager', \n                    align : 'start',\n                    value: 'relationships.caretakers.data.relationships' \n                },\n                { text: 'Actions', value: 'actions', sortable: false},\n            ],\n            propertyList : [],\n        }\n    },\n    created()  {\n        this.$nextTick(() => this.getPropertyList())\n    },\n    computed : {\n        ownerID () {\n            let id = this.$store.state.auth.profile.relationships.landlord.data.id;\n            console.log(id)\n            return id ?? null\n        },\n        ownerFullName() {\n            let data = this.$store.state.auth.profile.attributes\n            let name = data.firstName + \" \" + data.lastName\n            return name ?? null\n        },\n        list() {\n            let list = this.$store.state.propertyDetails.propertyList\n            return list;\n        }\n    },\n    methods:{\n        toggleAddProperty(){\n            this.showProperty = false;\n            this.addProperty = true;\n        },\n        saveStepOne() {\n            this.property.owner_id = this.ownerID\n            console.log(this.property)\n            this.e1 = 2\n        },\n        saveStepTwo() {\n            console.log(this.property)\n            this.e1 = 3\n        },\n        saveStepThree() {\n\n        },\n        stepColor(n) {\n            return this.e1 > n ? 'success' : 'primary';\n        },\n        step(s) { \n            if(this.e1 >= s) { this.e1 = s }\n        },\n        saveProperty() {\n            this.$store.dispatch('propertyDetails/createNewProperty', this.property).then(\n                success => {\n                    let data = success\n                    console.log(data)\n                    if(data) {\n                        this.addProperty = false;\n                        this.hasProperty = true;            \n                        this.showProperty = true;\n                    }\n                },\n                error => {\n                    if(error) {\n                        console.log(error)\n                    }\n                }\n            );            \n        },\n        getPropertyList() {\n            if(this.ownerID !== null) {\n                console.log(\"not null\")\n            } else {\n                console.log(\"this is null\")\n            }\n            this.$store.dispatch('propertyDetails/propertyList', this.ownerID).then(\n                list => {\n                    let data = list.data\n                    console.log(data)\n                    if(data.length >0) {\n                        this.propertyList = data\n                        this.cancelAddProperty()\n                    } \n                },\n                error => {\n                    console.log(error)\n                }\n            );\n        },\n        cancelAddProperty() {\n            this.addProperty = false\n            this.hasProperty = true                  \n            this.showProperty = true;\n        },\n        editItem(item) {\n            this.showModal = item ? !this.showModal : false\n        }\n    }\n}\n</script>"]}]}