ages=\"nameErrors\"\n                                        label=\"Address\"\n                                        required\n                                        @input=\"$v.name.$touch()\"\n                                        @blur=\"$v.name.$touch()\"\n                                    ></v-text-field>\n                                    <v-row> \n                                        <v-col>\n                                            <validation-provider\n                                                v-slot=\"{errors}\"\n                                                name=\"City/District\"\n                                            >\n                                                <v-text-field\n                                                    v-model=\"property.city\"\n                                                    :error-messages=\"errors\"\n                                                    label=\"City/District\"\n                                                    required\n                                                    @input=\"$v.name.$touch()\"\n                                                    @blur=\"$v.name.$touch()\"\n                                                ></v-text-field>\n                                            </validation-provider>\n                                        </v-col>\n                                        <v-col>\n                                            <validation-provider\n                                                v-slot=\"{errors}\"\n                                                name=\"County/State/Province\"\n                                            >\n                                                <v-text-field\n                                                    v-model=\"property.state\"\n                                                    :error-messages=\"errors\"\n                                                    label=\"County/State/Province\"\n     