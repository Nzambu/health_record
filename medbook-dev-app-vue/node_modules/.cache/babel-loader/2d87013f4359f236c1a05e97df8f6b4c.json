le\n                  <v-icon v-if=\"showProfile\"\n                    class=\"white--text\"                \n                  >mdi-pencil</v-icon>\n                  <v-icon v-if=\"!showProfile\"\n                    class=\"white--text\"                \n                  >mdi-close</v-icon>\n                </v-btn>\n              </template>\n              <span>Edit Profile</span>\n            </v-tooltip>\n              </v-row>            \n            </v-col>\n          </v-row> \n        </v-card-title>\n        <v-divider></v-divider>\n        <v-row>\n          <v-col lg=\"12\" class=\"text-center\">\n            <v-avatar size=\"200\">\n              <v-img src=\"https://gitlab.com/uploads/-/system/user/avatar/3883365/avatar.png\"></v-img>\n            </v-avatar>\n          </v-col>\n        </v-row>          \n        <v-row v-if=\"showProfile\">\n          <v-col lg=\"12\" class=\"text-center\">\n            <v-list>\n              <v-list-item two-line>\n                <v-list-item-title>\n                  {{profile.attributes.name}}\n                </v-list-item-title>\n              </v-list-item>\n              <v-list-item>\n                <v-list-item-title>\n                  {{profile.attributes.email}}\n                </v-list-item-title>\n              </v-list-item>\n            </v-list>\n          </v-col>\n        </v-row> \n        <v-row v-if=\"!showProfile\"\n          class=\"px-10\"\n        >\n          <v-col \n            lg=\"6\"\n            offset-lg=\"3\"\n          >\n            <v-form\n                lazy-validation\n            >\n              <validation-observer\n                  ref=\"profileObserver\"\n                  v-slot=\"{}\"\n              >\n                <validation-provider\n                  v-slot=\"{errors}\"\n                  name=\"First Name\"\n                  rules=\"required\"\n                >\n                  <v-text-field\n                      v-model=\"user.firstName\"\n   