DA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAPA;AAQA,KA7CA;;AA+CA;AACA;AACA;AACA,IAAA,oBAlDA,kCAkDA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KArDA;;AAuDA;AACA;AACA;AACA,IAAA,QA1DA,oBA0DA,IA1DA,EA0DA;AACA,WAAA,EAAA,GAAA,IAAA,GAAA,CAAA;AACA,UAAA,IAAA,GAAA,KAAA,aAAA;AACA,MAAA,IAAA,CAAA,OAAA,CAAA,UAAA,SAAA,EAAA;AACA,YAAA,IAAA,IAAA,SAAA,EAAA;AACA;AACA;AACA,OAJA;AAKA,WAAA,aAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAnEA;AAoEA,IAAA,YApEA,wBAoEA,IApEA,EAoEA;AACA,aAAA,KAAA,aAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AACA,KAtEA;AAuEA,IAAA,SAvEA,qBAuEA,IAvEA,EAuEA;AACA,aAAA,KAAA,aAAA,CAAA,QAAA,CAAA,IAAA,IAAA,SAAA,GAAA,SAAA;AACA,KAzEA;AA0EA,IAAA,IA1EA,gBA0EA,KA1EA,EA0EA;AAAA;;AACA,UAAA,IAAA,GAAA,KAAA,aAAA;AACA,MAAA,IAAA,CAAA,OAAA,CAAA,UAAA,SAAA,EAAA;AACA,YAAA,KAAA,KAAA,SAAA,EAAA;AACA,UAAA,MAAA,CAAA,EAAA,GAAA,KAAA;AACA;AACA;AACA,OALA;AAMA,KAlFA;;AAoFA;AACA;AACA;AACA,IAAA,QAvFA,oBAuFA,IAvFA,EAuFA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,KAzFA;;AA2FA;AACA;AACA;AACA,IAAA,UA9FA,sBA8FA,IA9FA,EA8FA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA;AAhGA;AAhFA,CAAA","sourcesContent":["<template>\n    <v-container>\n        <v-row v-if=\"showManagers\">\n            <v-col lg=\"12\">\n                <v-card color=\"light-blue\">\n                    <v-row class=\"px-10\">\n                    <v-col cols=\"12\" sm=\"12\" md=\"6\" lg=\"6\" color=\"success\">\n                        <v-select\n                            :items=\"propertyList\"\n                            :item-text=\"'attributes.name'\"\n                            :item-value=\"'id'\"\n                            disabled\n                            v-model=\"propertyID\"\n                            @input=\"selectedProperty\"\n                        ></v-select>\n                    </v-col>\n                    <v-col cols=\"12\" sm=\"12\" md=\"6\" lg=\"6\" color=\"success\">\n                        <v-row>\n                            <v-spacer></v-spacer>\n                            <v-btn class=\"mx-2\">\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"lime\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        >\n                                        mdi-book-open-variant\n                                        </v-icon>\n                                    </template>\n                                    <span>Items per page</span>\n                                </v-tooltip>\n                            </v-btn>\n                            <v-btn class=\"mx-2\" @click=\"toggleAddManager\">\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"green\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        \n                                        >\n                                        mdi-plus-circle\n                                        </v-icon>\n                                    </template>\n                                    <span>Add Unit</span>\n                                </v-tooltip>\n                            </v-btn>\n                            <v-btn class=\"mx-2\">\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"light-blue\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        >\n                                        mdi-clipboard-list\n                                        </v-icon>\n                                    </template>\n                                    <span>List Properties</span>\n                                </v-tooltip>\n                            </v-btn>\n                            <v-btn class=\"mx-2\">\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"primary\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        >\n                                        mdi-download\n                                        </v-icon>\n                                    </template>\n                                    <span>Export</span>\n                                </v-tooltip>\n                            </v-btn>\n                        </v-row>\n                    </v-col>\n                </v-row>\n                    <v-divider></v-divider>\n                    <div class=\"px-10 py-10\">\n                        <v-data-table\n                            :headers=\"headers\"\n                            :items=\"managersList\"\n                            :search=\"search\"\n                        >\n                            <template v-slot:item.actions=\"{ item }\">\n                                    <v-icon\n                                        medium\n                                        class=\"mr-2 blue--text\"\n                                        @click=\"editUnit(item)\"\n                                    >\n                                        mdi-pencil-circle\n                                    </v-icon>\n                                    <v-icon\n                                        medium\n                                        class=\"red--text\"\n                                        @click=\"deleteUnit(item)\"\n                                    >\n                                        mdi-delete\n                                    </v-icon>\n                                </template>\n                            <template v-slot:no-data>\n                                <div style=\"margin-top: 15%; margin-bottom: 15%;\">\n                                    <!-- <v-alert :value=\"true\" color=\"error\" icon=\"warning\">\n                                        Sorry, nothing to display here :(\n                                    </v-alert> -->\n                                    <v-row justify=\"center\">\n                                        <v-btn\n                                            color=\"success text-capitalize\"\n                                            rounded\n                                            @click=\"toggleAddManager\"                \n                                        >\n                                            <v-icon>mdi-plus-circle</v-icon> Add Manager\n                                        </v-btn>   \n                                    </v-row>\n                                </div>\n                            </template>\n                        </v-data-table>\n                    </div>\n                </v-card>\n            </v-col>\n        </v-row>\n        \n         <!-- Add new unit -->\n        <v-row v-if=\"addManager && !showManagers\">\n            <v-col \n                cols=\"12\"\n                sm=\"12\"\n                md=\"12\"\n                lg=\"12\"\n            >\n            <!-- Stepper wizard -->\n                <v-stepper v-model=\"e1\" alt-labels>\n                    <!-- Wizard header -->\n                    <v-stepper-header>\n                        <v-stepper-step\n                            :complete=\"completeStep(1)\"\n                            step=\"1\"\n                            :color=\"stepColor(1)\"\n                            @click=\"step(1)\"\n                        >\n                            Property Details\n                        </v-stepper-step>\n\n                        <v-divider></v-divider>\n\n                        <v-stepper-step\n                            :complete=\"completeStep(2)\"\n                            step=\"2\"\n                            :color=\"stepColor(2)\"\n                            @click=\"step(2)\"\n                        >\n                            Manager Details\n                        </v-stepper-step>\n                        \n                        <v-divider></v-divider>\n\n                        <v-stepper-step\n                            :complete=\"completeStep(3)\"\n                            step=\"3\"\n                            :color=\"stepColor(3)\"\n                            @click=\"step(3)\"\n                        >\n                            Status\n                        </v-stepper-step>\n\n                        <v-divider></v-divider>\n\n                        <v-stepper-step \n                            :complete=\"completeStep(4)\"\n                            step=\"4\"\n                            :color=\"stepColor(4)\"\n                            @click=\"step(4)\"\n                        >\n                            Preview &amp; Submit\n                            <small>Alert message</small>\n                        </v-stepper-step>\n                    </v-stepper-header>\n                    <!-- End Wizard header -->\n                    <!--  The wizard contents -->\n                    <v-stepper-items>\n                    <v-stepper-content step=\"1\" class=\"px-15 py-1 pb-15\">\n                        <validation-observer\n                            ref=\"stepOneObserver\"\n                        >\n                            <v-form class=\"px-15 py-15\">\n                                <div>\n                                    <validation-provider\n                                        v-slot=\"{errors}\"\n                                        name=\"Property\"\n                                        rules=\"required\"\n                                    >\n                                        <v-select\n                                            v-model=\"manager.property\"\n                                            :error-messages=\"errors\"\n                                            :items=\"items\"\n                                            label=\"Property\"\n                                            required\n                                            @change=\"$v.select.$touch()\"\n                                            @blur=\"$v.select.$touch()\"\n                                        ></v-select>\n                                    </validation-provider>\n                                    <v-menu\n                                        v-model=\"menu1\"\n                                        :close-on-content-click=\"false\"\n                                        max-width=\"290\"\n                                    >\n                                        <template v-slot:activator=\"{ on, attrs }\">\n                                            <validation-provider\n                                                v-slot=\"{errors}\"\n                                                name=\"Start Date\"\n                                                rules=\"required\"\n                                            >\n                                                <v-text-field\n                                                    :value=\"computedDateFormattedMomentjs\"\n                                                    :error-messages=\"errors\"\n                                                    clearable\n                                                    label=\"Starting Date\"\n                                                    readonly\n                                                    v-bind=\"attrs\"\n                                                    v-on=\"on\"\n                                                    @click:clear=\"date = null\"\n                                                ></v-text-field>\n                                            </validation-provider>\n                                        </template>\n                                        <v-date-picker\n                                            v-model=\"manager.date\"\n                                            @change=\"menu1 = false\"\n                                        ></v-date-picker>\n                                    </v-menu>\n                                </div>\n\n                                <!-- Continue and cancel buttons -->\n                                <v-btn \n                                    text \n                                    class=\"text-capitalize mr-2\"\n                                    @click=\"toggleAddManager\">\n                                Cancel\n                                </v-btn>\n                                <v-btn\n                                    color=\"success\"\n                                    class=\"text-capitalize\"\n                                    @click=\"saveStep(1)\"\n                                    :disabled=\"invalid\"\n                                >\n                                Continue\n                                </v-btn>\n                                <!-- end of buttons -->                        \n                            </v-form>\n                        </validation-observer>\n                    </v-stepper-content>\n                    <v-stepper-content step=\"2\"  class=\"px-15 py-10\">\n                        <validation-observer\n                            ref=\"stepOneObserver\"\n                        >\n                            <v-form  class=\"px-15 py-15\">\n                                <UserRegistration ref=\"manager\"/>\n\n                                <!-- Continue and cancel buttons -->\n                                <v-btn \n                                    text \n                                    class=\"text-capitalize mr-2\"\n                                    @click=\"toggleAddManager\"\n                                >\n                                    Cancel\n                                </v-btn>\n                                <v-btn\n                                    color=\"success\"\n                                    class=\"text-capitalize\"\n                                    @click=\"saveStep(2)\"\n                                    :disabled=\"invalid\"\n                                >\n                                    Continue\n                                </v-btn>\n                                <!-- end of buttons --> \n                            </v-form>\n                        </validation-observer>\n                    </v-stepper-content>\n                    <v-stepper-content step=\"3\">\n                        <validation-observer>\n                            <v-form>\n                                \n                                <Sign ref=\"signature\"/>\n\n                                <!-- Continue and cancel buttons -->\n                                <v-btn \n                                    text \n                                    class=\"text-capitalize mr-2\"\n                                    @click=\"toggleAddManager\"\n                                >\n                                    Cancel\n                                </v-btn>\n                                <v-btn\n                                    color=\"success\"\n                                    class=\"text-capitalize\"\n                                    @click=\"saveStep(3)\"\n                                    :disabled=\"invalid\"\n                                >\n                                    Continue\n                                </v-btn>\n                                <!-- end of buttons -->\n                            </v-form>\n                        </validation-observer>\n                    </v-stepper-content>\n                    <v-stepper-content step=\"4\">\n\n                        <!-- Continue and cancel buttons -->\n                        <v-btn \n                            text \n                            class=\"text-capitalize mr-2\"\n                            @click=\"toggleAddManager\"\n                        >\n                            Cancel\n                        </v-btn>\n                        <v-btn\n                            color=\"success\"\n                            class=\"text-capitalize\"\n                            @click=\"saveStep(4)\"\n                            :disabled=\"invalid\"\n                        >\n                            Continue\n                        </v-btn>\n                        <!-- end of buttons -->\n                    </v-stepper-content>\n                    </v-stepper-items>\n                </v-stepper>\n            <!-- End stepper wizard -->\n            </v-col>\n        </v-row>\n        <!-- End add new unit -->\n    </v-container>\n</template>\n<script>\nimport ManagerModel from '../models/ManagerModel';\nimport Sign from '../components/Sign.vue';\nimport UserRegistration from '../components/UserRegistration.vue';\nimport moment from 'moment'\nimport { format, parseISO } from 'date-fns'\n\nexport default {\n  components: { Sign, UserRegistration },\n    name : 'Manager',\n    data: () => ({\n        showManagers : true,\n        addManager : false,\n        manager : new ManagerModel(),\n        e1 : 1,\n        completeSteps: [],\n        selectedPropertyID : null,\n        managersList : [],\n        headers: [\n            {\n                text: 'ID',\n                align: 'start',\n                sortable: false,\n                value : 'id'\n            },\n            {\n                text: 'Unit',\n                align: 'start',\n                sortable: false,\n                value : 'relationships.user.data[0].attributes.firstName'\n            },\n            {\n                text: 'Type',\n                align: 'start',\n                value : 'attributes.type'\n            },\n            {\n                text: 'Size',\n                align: 'start',\n                value : 'attributes.size'\n            },\n            {\n                text: 'Floor',\n                align: 'start'\n            },\n            {\n                text: 'Status',\n                align: 'start'\n            },\n            { text: 'Actions', value: 'actions', sortable: false},\n        ],\n        items : [1,2,3,4,5],\n        // https://github.com/date-fns/date-fns/blob/master/docs/upgradeGuide.md#string-arguments\n        date: format(parseISO(new Date().toISOString()), 'yyyy-MM-dd'),\n        menu1: false,\n        menu2: false,\n    }),\n    computed: {\n      computedDateFormattedMomentjs () {\n        return this.date ? moment(this.date).format('dddd, MMMM Do YYYY') : ''\n      },\n      computedDateFormattedDatefns () {\n        return this.date ? format(this.date, 'EEEE, MMMM do yyyy') : ''\n      },\n      propertyList () {\n          return this.$store.state.propertyDetails.propertyList\n      },\n      propertyID() {\n          return this.$store.state.propertyDetails.selectedProperty.id\n        //   return this.propertyList[0].id\n      }\n    },\n    watch : {\n        propertyID(newPropertyID, oldPropertyID) {\n            console.log(newPropertyID, oldPropertyID)\n            this.getManagers();\n        }\n    },\n    created()  {\n        this.$nextTick(() => { \n            this.getManagers()\n        });\n    },\n    mounted() {\n        this.undo()\n        this.save()\n    },\n    methods: {\n        toggleAddManager() {\n            this.addManager = !this.addManager\n            this.showManagers = !this.showManagers\n        },\n\n        /**\n         * \n         */\n        undo() {\n        this.$refs.signaturePad.undoSignature();\n        },\n\n        /**\n         * \n         */\n        save() {\n            console.log(\"this is save signature\")\n            // return\n            const { isEmpty, data } = this.$refs.signaturePad.saveSignature();\n            console.log(isEmpty);\n            console.log(data);\n        },\n\n        /**\n         * Select property where to get managers\n         */\n        selectedProperty(item) {\n            this.selectedPropertyID = item;\n            this.getManagers();\n        },\n\n        /**\n         * \n         */\n        getManagers() {\n            let propertyID = this.selectedPropertyID ?? this.propertyID\n            this.$store.dispatch('managerDetails/getPropertyManagers', propertyID)\n            .then((managers) => {\n                this.managersList = managers.data\n                console.log(managers)\n            })\n            .catch((error) => {\n                    console.log(error)\n            });\n        },\n\n        /**\n         * Cancel add new unit\n         */\n        cancelSaveNewManager() {\n            this.addUnits = false\n            this.showUnits = true\n        },\n\n        /**\n         * Wizard/stepper\n         */\n        saveStep(step) { \n            this.e1 = step+1            \n            let data = this.completeSteps\n            data.forEach((savedStep) => {\n                if(step == savedStep){\n                    return\n                }                \n            })\n            this.completeSteps.push(step)\n        },\n        completeStep(step) {\n            return this.completeSteps.includes(step)\n        },\n        stepColor(step) {\n            return this.completeSteps.includes(step) ? 'success' : 'primary'\n        },\n        step(step) {\n            let data = this.completeSteps\n            data.forEach((savedStep) => {\n                if(step === savedStep){\n                    this.e1 = step;\n                    return\n                }\n            })\n        },\n\n        /**\n         * Edit unit\n         */\n        editUnit(unit) {\n            console.log(unit)\n        },\n\n        /**\n         * Delete unit\n         */\n        deleteUnit(unit) {\n            console.log(unit)\n        }\n    }\n}\n</script>"],"sourceRoot":"src/views"}]}