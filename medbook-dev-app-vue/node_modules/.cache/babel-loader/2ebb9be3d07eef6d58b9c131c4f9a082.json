A,EAAA,KAAA,IAAA,CAAA,WAAA;AACA,KAVA;AAWA,IAAA,WAXA,yBAWA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,UAAA,CAAA,WAAA,GAAA,IAAA,CAAA,UAAA,OAAA,EAAA;AACA,YAAA,CAAA,OAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,4BAAA;AACA;AACA;;AAEA,YAAA,KAAA,CAAA,IAAA,CAAA,QAAA,IAAA,KAAA,CAAA,IAAA,CAAA,YAAA,IAAA,KAAA,CAAA,IAAA,CAAA,WAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,YAAA,EAAA,KAAA,CAAA,IAAA,EAAA,IAAA,CACA,YAAA;AACA;AACA,YAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,WAJA,EAKA,UAAA,KAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,YAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,YAAA,KAAA,CAAA,OAAA,GACA,KAAA,CAAA,QAAA,IAAA,KAAA,CAAA,QAAA,CAAA,IAAA,IACA,KAAA,CAAA,OADA,IAEA,KAAA,CAAA,QAAA,EAHA;AAIA,WAZA;AAcA;AACA,OAvBA;AAwBA;AArCA;AA5BA,CAAA","sourcesContent":["<template>\n    <v-container> \n        <v-row>\n            <v-col \n                offset-sm=\"1\"\n                sm=\"10\"\n                offset-md=\"2\"\n                md=\"8\"\n                offset-lg=\"4\"\n                lg=\"4\"                \n            >\n                <v-card class=\"px-10 py-10 rounded-lg\">\n                    <v-card-text class=\"pb-10\">\n                        <h1 class=\"text-center primary--text\">Sign in to Vuex</h1>\n                    </v-card-text>\n                    <v-form\n                        ref=\"form\"\n                        v-model=\"valid\"\n                        lazy-validation\n                    >\n                        <v-text-field\n                        v-model=\"user.userName\"\n                        :rules=\"nameRules\"\n                        label=\"Name\"\n                        required\n                        ></v-text-field>\n\n                        <v-text-field\n                        v-model=\"user.userPassword\"\n                        :rules=\"passwordRules\"\n                        label=\"Password\"\n                        required\n                        ></v-text-field>\n\n                        <v-text-field\n                        v-model=\"user.companyCode\"\n                        :rules=\"codeRules\"\n                        label=\"Company code\"\n                        required\n                        ></v-text-field>\n\n                        <v-col class=\"text-right pr-0 pt-0 pb-0\">\n                            <v-btn\n                                :disabled=\"!valid\"\n                                color=\"success\"\n                                class=\"mr-0\"\n                                @click=\"handleLogin\"\n                            >\n                            LOGIN\n                            </v-btn>\n                        </v-col>\n                    </v-form>\n                </v-card>\n                <v-col>\n                    <p class=\"text-center\"><small>Copyright &copy; 2020 <span class=\"primary--text\">Vue and Vuex</span></small></p>\n                </v-col>\n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n\n<script>\nimport AuthService from '../services/AuthService.js'\nimport User from '../models/User'\nexport default {\n    name : 'Login',\n    data () {\n        return {\n            user : new User('', '', ''),\n            authService : AuthService,\n            valid: true,\n            name: '',\n            nameRules: [\n                v => !!v || 'Name is required',\n                v => (v && v.length <= 10) || 'Name must be less than 10 characters',\n            ],\n            email: '',\n            passwordRules: [\n                v => !!v || 'Password is required',\n                v => (v && v.length >= 8) || 'Password must be equal to or longer that 8 characters',\n            ],\n            codeRules: [\n                v => !!v || 'Company code is required',\n                v => (v && v.length <= 3) || 'Name must be more than 3 characters',\n            ],\n        }\n    },\n    computed: {\n        loggedIn() {\n            return this.$store.state.auth.status.loggedIn;\n            }\n        },\n    methods : {\n        signIn(){\n            // let data = {\n            //     // \"companyCode\" : \"dev\",\n            //     \"userName\" : \"Nzioki\",\n            //     \"userPassword\" : \"password\"\n            // }\n            // var feedback = this.authService.login(data)\n            // console.log(feedback.status)\n            console.log(this.user.userName, this.user.userPassword, this.user.companyCode)\n        },\n        handleLogin() {\n            this.loading = true;\n            this.$validator.validateAll().then(isValid => {\n                if (!isValid) {\n                    this.loading = false;\n                    console.log(\"This is not working at all\")\n                    return;\n                }\n\n                if (this.user.userName && this.user.userPassword && this.user.companyCode) {\n                    this.$store.dispatch('auth/login', this.user).then(\n                        () => {\n                            // console.log(\"credentioals\")\n                            this.$router.push('/about');\n                        },\n                        error => {\n                            console.log(error)\n                        this.loading = false;\n                        this.message =\n                            (error.response && error.response.data) ||\n                            error.message ||\n                            error.toString();\n                        }\n                    )\n                }\n            });\n        }\n    }\n}\n</script>"],"sourceRoot":"src/components"}]}