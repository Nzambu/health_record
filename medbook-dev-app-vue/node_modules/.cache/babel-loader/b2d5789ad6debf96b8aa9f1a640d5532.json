,EAAA,OADA;AAEA,QAAA,KAAA,EAAA;AAFA,OAjBA,EAqBA;AACA,QAAA,IAAA,EAAA,QADA;AAEA,QAAA,KAAA,EAAA;AAFA,OArBA,EAyBA;AAAA,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,QAAA,EAAA;AAAA,OAzBA;AAVA,KAAA;AAsCA,GAzCA;AA0CA,EAAA,OA1CA,qBA0CA;AAAA;;AACA,SAAA,SAAA,CAAA;AAAA,aAAA,KAAA,CAAA,QAAA,EAAA;AAAA,KAAA;AACA,GA5CA;AA6CA,EAAA,QAAA,EAAA;AACA;AACA;AACA;AACA,IAAA,QAJA,sBAIA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,eAAA,CAAA,gBAAA;AACA,KANA;;AAQA;AACA;AACA;AACA,IAAA,UAXA,wBAWA;AACA,aAAA,KAAA,QAAA,CAAA,EAAA;AACA,KAbA;;AAeA;AACA;AACA;AACA,IAAA,YAlBA,0BAkBA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,eAAA,CAAA,YAAA;AACA,KApBA;;AAsBA;AACA;AACA;AACA,IAAA,iBAzBA,+BAyBA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,eAAA,CAAA,YAAA,CAAA,CAAA,EAAA,EAAA;AACA;AA3BA,GA7CA;AA0EA,EAAA,KAAA,EAAA;AACA,IAAA,UADA,sBACA,aADA,EACA,aADA,EACA;AACA,UAAA,aAAA,KAAA,aAAA,EAAA;AACA,aAAA,QAAA;AACA;AACA;AALA,GA1EA;AAiFA,EAAA,OAAA,EAAA;AACA;AACA;AACA;AACA,IAAA,gBAJA,4BAIA,IAJA,EAIA;AACA,WAAA,kBAAA,GAAA,IAAA;AACA,WAAA,QAAA;AACA,KAPA;;AASA;AACA;AACA;AACA,IAAA,QAZA,sBAYA;AAAA;;AACA,UAAA,UAAA,GAAA,KAAA,UAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,8BAAA,EAAA,UAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,gBAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OANA;AAOA,KArBA;;AAuBA;AACA;AACA;AACA,IAAA,aA1BA,2BA0BA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,eAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,KA9BA;;AAgCA;AACA;AACA;AACA,IAAA,WAnCA,yBAmCA;AACA,WAAA,iBAAA;AACA,KArCA;;AAuCA;AACA;AACA;AACA,IAAA,iBA1CA,+BA0CA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KA7CA;;AA+CA;AACA;AACA;AACA,IAAA,QAlDA,oBAkDA,IAlDA,EAkDA;AACA,WAAA,EAAA,GAAA,IAAA,GAAA,CAAA;AACA,UAAA,IAAA,GAAA,KAAA,aAAA;AACA,MAAA,IAAA,CAAA,OAAA,CAAA,UAAA,SAAA,EAAA;AACA,YAAA,IAAA,IAAA,SAAA,EAAA;AACA;AACA;AACA,OAJA;AAKA,WAAA,aAAA,CAAA,IAAA,CAAA,IAAA;AACA,KA3DA;AA4DA,IAAA,YA5DA,wBA4DA,IA5DA,EA4DA;AACA,aAAA,KAAA,aAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AACA,KA9DA;AA+DA,IAAA,SA/DA,qBA+DA,IA/DA,EA+DA;AACA,aAAA,KAAA,aAAA,CAAA,QAAA,CAAA,IAAA,IAAA,SAAA,GAAA,SAAA;AACA,KAjEA;AAkEA,IAAA,IAlEA,gBAkEA,KAlEA,EAkEA;AAAA;;AACA,UAAA,IAAA,GAAA,KAAA,aAAA;AACA,MAAA,IAAA,CAAA,OAAA,CAAA,UAAA,SAAA,EAAA;AACA,YAAA,KAAA,KAAA,SAAA,EAAA;AACA,UAAA,MAAA,CAAA,EAAA,GAAA,KAAA;AACA;AACA;AACA,OALA;AAMA,KA1EA;;AA4EA;AACA;AACA;AACA,IAAA,QA/EA,oBA+EA,IA/EA,EA+EA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,UAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,KAtFA;;AAwFA;AACA;AACA;AACA,IAAA,UA3FA,sBA2FA,IA3FA,EA2FA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA;AA7FA;AAjFA,CAAA","sourcesContent":["<template>\n    <v-container>\n        <v-row v-if=\"hasUnits && showUnits\">\n            <v-col lg=\"12\">\n            <v-card>\n                <v-row class=\"px-10\">\n                    <v-col cols=\"12\" sm=\"12\" md=\"6\" lg=\"6\" color=\"success\">\n                        <v-select\n                            :items=\"propertyList\"\n                            :item-text=\"'attributes.name'\"\n                            :item-value=\"'id'\"\n                            disabled\n                            v-model=\"propertyID\"\n                            @input=\"selectedProperty\"\n                        ></v-select>\n                    </v-col>\n                    <v-col cols=\"12\" sm=\"12\" md=\"6\" lg=\"6\" color=\"success\">\n                        <v-row>\n                            <v-spacer></v-spacer>\n                            <v-btn class=\"mx-2\">\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"lime\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        >\n                                        mdi-book-open-variant\n                                        </v-icon>\n                                    </template>\n                                    <span>Items per page</span>\n                                </v-tooltip>\n                            </v-btn>\n                            <v-btn class=\"mx-2\">\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"green\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        @click=\"toggleAddUnit\"\n                                        >\n                                        mdi-plus-circle\n                                        </v-icon>\n                                    </template>\n                                    <span>Add Unit</span>\n                                </v-tooltip>\n                            </v-btn>\n                            <v-btn class=\"mx-2\">\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"light-blue\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        >\n                                        mdi-clipboard-list\n                                        </v-icon>\n                                    </template>\n                                    <span>List Properties</span>\n                                </v-tooltip>\n                            </v-btn>\n                            <v-btn class=\"mx-2\">\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"primary\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        >\n                                        mdi-download\n                                        </v-icon>\n                                    </template>\n                                    <span>Export</span>\n                                </v-tooltip>\n                            </v-btn>\n                        </v-row>\n                    </v-col>\n                </v-row>\n                <v-divider></v-divider>\n                <div class=\"px-10 py-10\">\n                    <v-data-table\n                        :headers=\"headers\"\n                        :items=\"propertyUnitList\"\n                        :search=\"search\"\n                    >\n                        <template v-slot:item.actions=\"{ item }\">\n                                <v-icon\n                                    medium\n                                    class=\"mr-2 blue--text\"\n                                    @click=\"editUnit(item)\"\n                                >\n                                    mdi-pencil-circle\n                                </v-icon>\n                                <v-icon\n                                    medium\n                                    class=\"red--text\"\n                                    @click=\"deleteUnit(item)\"\n                                >\n                                    mdi-delete\n                                </v-icon>\n                            </template>\n                        <template v-slot:no-data>\n                            <div style=\"margin-top: 15%; margin-bottom: 15%;\">\n                                <!-- <v-alert :value=\"true\" color=\"error\" icon=\"warning\">\n                                    Sorry, nothing to display here :(\n                                </v-alert> -->\n                                <v-row justify=\"center\">\n                                    <v-btn\n                                        color=\"success text-capitalize\"\n                                        rounded\n                                        @click=\"toggleAddUnit\"                \n                                    >\n                                        <v-icon>mdi-plus-circle</v-icon> Add Unit\n                                    </v-btn>   \n                                </v-row>\n                            </div>\n                        </template>\n                    </v-data-table>\n                </div>\n            </v-card>\n            </v-col>\n        </v-row>\n        <v-row>\n            \n        </v-row>\n        <!-- Add new unit -->\n        <v-row v-if=\"addUnits\">\n            <v-col \n                cols=\"12\"\n                sm=\"12\"\n                md=\"12\"\n                lg=\"12\"\n            >\n            <!-- Stepper wizard -->\n                <v-stepper v-model=\"e1\" alt-labels>\n                    <!-- Wizard header -->\n                    <v-stepper-header>\n                        <v-stepper-step\n                            :complete=\"completeStep(1)\"\n                            step=\"1\"\n                            :color=\"stepColor(1)\"\n                            @click=\"step(1)\"\n                        >\n                            Property Details\n                        </v-stepper-step>\n\n                        <v-divider></v-divider>\n\n                        <v-stepper-step\n                            :complete=\"completeStep(2)\"\n                            step=\"3\"\n                            :color=\"stepColor(2)\"\n                            @click=\"step(2)\"\n                        >\n                            Unit Details\n                        </v-stepper-step>\n                        \n                        <v-divider></v-divider>\n\n                        <v-stepper-step\n                            :complete=\"completeStep(3)\"\n                            step=\"4\"\n                            :color=\"stepColor(3)\"\n                            @click=\"step(3)\"\n                        >\n                            Status\n                        </v-stepper-step>\n\n                        <v-divider></v-divider>\n\n                        <v-stepper-step \n                            :complete=\"completeStep(4)\"\n                            step=\"4\"\n                            :color=\"stepColor(4)\"\n                            @click=\"step(4)\"\n                        >\n                            Preview &amp; Submit\n                            <small>Alert message</small>\n                        </v-stepper-step>\n                    </v-stepper-header>\n                    <!-- End Wizard header -->\n                    <!--  The wizard contents -->\n                    <v-stepper-items>\n                    <v-stepper-content step=\"1\" class=\"py-10 px-15\">\n                        <validation-observer\n                            ref=\"stepOneObserver\"\n                        >\n                            <v-form class=\"py-15 px-15\">\n                                <validation-provider\n                                    v-slot=\"{errors}\"\n                                    name=\"Property\"\n                                    rules=\"required\"\n                                >\n                                    <v-select\n                                        v-model=\"propertyID\"\n                                        :error-messages=\"errors\"\n                                        :items=\"propertyList\"\n                                        :item-text=\"'attributes.name'\"\n                                        :item-value=\"'id'\"\n                                        label=\"Apartment\"\n                                        required\n                                        @change=\"$v.select.$touch()\"\n                                        @blur=\"$v.select.$touch()\"\n                                    ></v-select>\n                                </validation-provider>\n                                <validation-provider\n                                    v-slot=\"{errors}\"\n                                    name=\"Block/Wing\"\n                                >\n                                    <v-select\n                                        v-model=\"select\"\n                                        :error-messages=\"errors\"\n                                        :items=\"items\"\n                                        label=\"Block/Wing\"\n                                        required\n                                        @change=\"$v.select.$touch()\"\n                                        @blur=\"$v.select.$touch()\"\n                                    ></v-select>\n                                </validation-provider>\n                                <validation-provider\n                                    v-slot=\"{errors}\"\n                                    name=\"Floor\"\n                                >\n                                    <v-select\n                                        v-model=\"select\"\n                                        :error-messages=\"errors\"\n                                        :items=\"items\"\n                                        label=\"Floor\"\n                                        required\n                                        @change=\"$v.select.$touch()\"\n                                        @blur=\"$v.select.$touch()\"\n                                    ></v-select>\n                                </validation-provider>\n                                <!-- Continue and cancel buttons -->\n                                <v-btn \n                                    text \n                                    class=\"text-capitalize mr-2\"\n                                    @click=\"cancelSaveNewUnit\">\n                                Cancel\n                                </v-btn>\n                                <v-btn\n                                    color=\"success\"\n                                    class=\"text-capitalize\"\n                                    @click=\"saveStep(1)\"\n                                    :disabled=\"invalid\"\n                                >\n                                Continue\n                                </v-btn>\n                                <!-- end of buttons -->                        \n                            </v-form>\n                        </validation-observer>\n                    </v-stepper-content>\n                    <v-stepper-content step=\"2\" class=\"py-10 px-15\">\n                        <validation-observer\n                            ref=\"stepOneObserver\"\n                        >\n                            <v-form class=\"py-15 px-15\">\n                                <validation-provider\n                                    v-slot=\"{errors}\"\n                                    name=\"Name\"\n                                    rules=\"required\"\n                                >\n                                    <v-text-field\n                                        v-model=\"unit.name\"\n                                        :error-messages=\"errors\"\n                                        label=\"Name\"\n                                        required\n                                        @input=\"$v.name.$touch()\"\n                                        @blur=\"$v.name.$touch()\"\n                                    ></v-text-field>\n                                </validation-provider>\n                                <validation-provider\n                                    v-slot=\"{errors}\"\n                                    name=\"Area\"\n                                >\n                                    <v-text-field\n                                        v-model=\"unit.area\"\n                                        :error-messages=\"errors\"\n                                        label=\"Area(in Sq. ft)\"\n                                        required\n                                        @input=\"$v.name.$touch()\"\n                                        @blur=\"$v.name.$touch()\"\n                                    ></v-text-field>\n                                </validation-provider>\n                                <validation-provider\n                                    v-slot=\"{errors}\"\n                                    name=\"Type\"\n                                    rules=\"required\"\n                                >\n                                    <v-select\n                                        v-model=\"unit.type\"\n                                        :items=\"items\"\n                                        :error-messages=\"errors\"\n                                        label=\"Type of the unit\"\n                                        required\n                                        @change=\"$v.select.$touch()\"\n                                        @blur=\"$v.select.$touch()\"\n                                    ></v-select>\n                                </validation-provider>\n                                <validation-provider\n                                    v-slot=\"{errors}\"\n                                    name=\"Bedrooms\"\n                                >\n                                    <v-text-field\n                                        v-model=\"name\"\n                                        :error-messages=\"errors\"\n                                        label=\"Bedrooms\"\n                                        required\n                                        @input=\"$v.name.$touch()\"\n                                        @blur=\"$v.name.$touch()\"\n                                    ></v-text-field>\n                                </validation-provider>\n                                <validation-provider\n                                    v-slot=\"{errors}\"\n                                    name=\"Bathrooms\"\n                                >\n                                    <v-text-field\n                                        v-model=\"name\"\n                                        :error-messages=\"errors\"\n                                        label=\"Bathrooms\"\n                                        required\n                                        @input=\"$v.name.$touch()\"\n                                        @blur=\"$v.name.$touch()\"\n                                    ></v-text-field>\n                                </validation-provider>\n                                <validation-provider\n                                    v-slot=\"{errors}\"\n                                    name=\"Kitchens\"\n                                >\n                                    <v-text-field\n                                        v-model=\"name\"\n                                        :error-messages=\"errors\"\n                                        label=\"Kitchens\"\n                                        required\n                                        @input=\"$v.name.$touch()\"\n                                        @blur=\"$v.name.$touch()\"\n                                    ></v-text-field>\n                                </validation-provider>\n                                <!-- Continue and cancel buttons -->\n                                <v-btn \n                                    text \n                                    class=\"text-capitalize mr-2\"\n                                    @click=\"cancelSaveNewUnit\"\n                                >\n                                    Cancel\n                                </v-btn>\n                                <v-btn\n                                    color=\"success\"\n                                    class=\"text-capitalize\"\n                                    @click=\"saveStep(2)\"\n                                    :disabled=\"invalid\"\n                                >\n                                    Continue\n                                </v-btn>\n                                <!-- end of buttons --> \n                            </v-form>\n                        </validation-observer>\n                    </v-stepper-content>\n                    <v-stepper-content step=\"3\" class=\"py-10 px-15\">\n                        <validation-observer>\n                            <v-form class=\"py-15 px-15\">\n                                <validation-provider\n                                    v-slot=\"{errors}\"\n                                    name=\"Status\"\n                                    rules=\"required\"\n                                >\n                                    <v-text-field\n                                        v-model=\"name\"\n                                        :error-messages=\"errors\"\n                                        label=\"Status\"\n                                        required\n                                        @input=\"$v.name.$touch()\"\n                                        @blur=\"$v.name.$touch()\"\n                                    ></v-text-field>\n                                </validation-provider>\n                                <validation-provider\n                                    v-slot=\"{errors}\"\n                                    name=\"Status\"\n                                >\n                                    <v-text-field\n                                        v-model=\"name\"\n                                        :error-messages=\"errors\"\n                                        label=\"Amenities\"\n                                        required\n                                        @input=\"$v.name.$touch()\"\n                                        @blur=\"$v.name.$touch()\"\n                                    ></v-text-field>\n                                </validation-provider>\n                                <!-- Continue and cancel buttons -->\n                                <v-btn \n                                    text \n                                    class=\"text-capitalize mr-2\"\n                                    @click=\"cancelSaveNewUnit\"\n                                >\n                                    Cancel\n                                </v-btn>\n                                <v-btn\n                                    color=\"success\"\n                                    class=\"text-capitalize\"\n                                    @click=\"saveStep(3)\"\n                                    :disabled=\"invalid\"\n                                >\n                                    Continue\n                                </v-btn>\n                                <!-- end of buttons -->\n                            </v-form>\n                        </validation-observer>\n                    </v-stepper-content>\n                    <v-stepper-content step=\"4\">\n\n                        <!-- Continue and cancel buttons -->\n                        <v-btn \n                            text \n                            class=\"text-capitalize mr-2\"\n                            @click=\"cancelSaveNewUnit\"\n                        >\n                            Cancel\n                        </v-btn>\n                        <v-btn\n                            color=\"success\"\n                            class=\"text-capitalize\"\n                            @click=\"saveStep(4)\"\n                            :disabled=\"invalid\"\n                        >\n                            Continue\n                        </v-btn>\n                        <!-- end of buttons -->\n                    </v-stepper-content>\n                    </v-stepper-items>\n                </v-stepper>\n            <!-- End stepper wizard -->\n            </v-col>\n        </v-row>\n        <!-- End add new unit -->\n    </v-container>\n</template>\n<script>\nimport UnitModel from '../models/UnitModel';\nexport default {\n    name : 'Unit',\n    data() {\n        return {\n            unit : new UnitModel(),\n            hasUnits : true,\n            addUnits : false,\n            showUnits : true,\n            e1 : 1,\n            completeSteps : [],\n            items : ['item one', 'item two', 'item three', 'item four', 'item five'],\n            selectedPropertyID : null,\n            propertyUnitList : [],\n            headers: [\n                {\n                    text: 'Unit',\n                    align: 'start',\n                    sortable: false,\n                    value : 'attributes.name'\n                },\n                {\n                    text: 'Type',\n                    align: 'start',\n                    value : 'attributes.type'\n                },\n                {\n                    text: 'Size',\n                    align: 'start',\n                    value : 'attributes.size'\n                },\n                {\n                    text: 'Floor',\n                    align: 'start'\n                },\n                {\n                    text: 'Status',\n                    align: 'start'\n                },\n                { text: 'Actions', value: 'actions', sortable: false},\n            ]\n        }\n    },\n    created()  {\n        this.$nextTick(() => this.getUnits())\n    },\n    computed : {\n        /**\n         * First property in list as default\n         */\n        property() {\n            return this.$store.state.propertyDetails.selectedProperty            \n        },\n\n        /**\n         * Get propertyID\n         */\n        propertyID() {\n            return this.property.id\n        },\n\n        /**\n         * Property list\n         */\n        propertyList() {\n            return this.$store.state.propertyDetails.propertyList;\n        },\n        \n        /**\n         * Default property ID\n         */\n        defaultPropertyID() {\n            return this.$store.state.propertyDetails.propertyList[0].id\n        }\n    },\n    watch : {\n        propertyID(newPropertyID, oldPropertyID) {\n            if(newPropertyID !== oldPropertyID) {\n                this.getUnits();\n            }\n        }\n    },\n    methods : {\n        /**\n         * Select property where to get units\n         */\n        selectedProperty(item) {\n            this.selectedPropertyID = item;\n            this.getUnits();\n        },\n\n        /**\n         * Select units for a particular property\n         */\n        getUnits() {\n            let propertyID = this.propertyID\n            this.$store.dispatch('unitDetails/getPropertyUnits', propertyID)\n            .then(response => {\n                this.propertyUnitList = response.data\n            })\n            .catch(error => {\n                console.log(error)\n            });\n        },\n        \n        /**\n         * Show or hide add new unit\n         */\n        toggleAddUnit() {\n            console.log(this.defaultProperty)\n            this.addUnits = true\n            this.showUnits = false\n        },\n\n        /**\n         * Submit data for new unit\n         */\n        saveNewUnit() {\n            this.cancelSaveNewUnit()\n        },\n\n        /**\n         * Cancel add new unit\n         */\n        cancelSaveNewUnit() {\n            this.addUnits = false\n            this.showUnits = true\n        },\n\n        /**\n         * Wizard/stepper\n         */\n        saveStep(step) { \n            this.e1 = step+1            \n            let data = this.completeSteps\n            data.forEach((savedStep) => {\n                if(step == savedStep){\n                    return\n                }                \n            })\n            this.completeSteps.push(step)\n        },\n        completeStep(step) {\n            return this.completeSteps.includes(step)\n        },\n        stepColor(step) {\n            return this.completeSteps.includes(step) ? 'success' : 'primary'\n        },\n        step(step) {\n            let data = this.completeSteps\n            data.forEach((savedStep) => {\n                if(step === savedStep){\n                    this.e1 = step;\n                    return\n                }\n            })\n        },\n\n        /**\n         * Edit unit\n         */\n        editUnit(unit) {\n            let data = unit.attributes\n            this.unit.name = data.name\n            this.unit.area = data.size\n            this.unit.type = data.type\n            this.addUnits = true\n            this.showUnits = false\n        },\n\n        /**\n         * Delete unit\n         */\n        deleteUnit(unit) {\n            console.log(unit)\n        }\n    }\n}\n</script>"],"sourceRoot":"src/views"}]}