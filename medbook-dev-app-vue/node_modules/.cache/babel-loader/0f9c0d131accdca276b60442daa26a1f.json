ll,\n                        languages : null,\n                        company : null,\n                        companies : null\n                    }\n\nexport const userDetails = {\n    namespaced: true,\n    state: initialState,\n    actions: {\n        language({ commit }) {\n            return UserService.language(user.id).then(\n                language => {\n                    commit('languageSuccess', language.data.data);\n                    return Promise.resolve(language);\n                },\n                error => {\n                    commit('languageFailure');\n                    return Promise.reject(error);\n                }\n            );\n        },\n        languages({ commit }) {\n            return UserService.languages(user.id).then(\n                languages => {\n                    commit('languagesSuccess', languages.data.data);\n                    return Promise.resolve(languages);\n                },\n                error => {\n                    commit('languagesFailure');\n                    return Promise.reject(error);\n                }\n            );\n        },\n        company({ commit }) {\n            return UserService.company(user.id).then(\n                company => {\n                    commit('companySuccess', company.data.data);\n                    return Promise.resolve(company);\n                },\n                error => {\n                    commit('companyFailure');\n                    return Promise.resolve(error);\n                }\n            )\n        },\n        companies({ commit }) {\n            return UserService.companies(user.id).then(\n                companies => {\n                    commit('companiesSuccess', companies.data.data);\n                    return Promise.resolve(companies);\n                },\n                error => {\n                    commit('companiesFailure');\n                    return Promise.resolve(error);\n                }\n            )\n        },\n    },\n    mutations: {\n        languageSuccess(state, language) {\n            state.language = language;\n        },\n        languageFailure(state) {\n            state.language = null;\n        },\n        languagesSuccess(state, languages) {\n            state.languages = languages;\n        },\n        languagesFailure(state) {\n            state.languages = null;\n        },\n        companySuccess(state, company) {\n            state.company = company;\n        },\n        companyFailure(state) {\n            state.company = null;\n        },\n        companiesSuccess(state, companies) {\n            state.companies = companies;\n        },\n        companiesFailure(state) {\n            state.companies = null;\n        }\n    }\n}\n"]}]}