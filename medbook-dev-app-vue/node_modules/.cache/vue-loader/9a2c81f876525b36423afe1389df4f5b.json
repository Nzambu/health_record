           <v-icon\n                      dark\n                      v-bind=\"attrs\"\n                      v-on=\"on\"\n                    >\n                    mdi-menu-down\n                    </v-icon>\n                  </v-btn> -->\n                  <v-list-item One-line\n                    v-bind=\"attrs\"\n                    v-on=\"on\"\n                  >\n                  <v-list-item-content>\n                      <v-list-item-subtitle>{{propertyName}}\n                        <v-icon\n                          color=\"grey\"\n                          \n                        >\n                          mdi-menu-down\n                        </v-icon>    \n                      </v-list-item-subtitle>\n                  </v-list-item-content>\n              </v-list-item> \n                </template>\n                <v-list>\n                  <v-list-item\n                    v-for=\"(item, index) in propertyList\"\n                    :key=\"index\"\n                    @click=\"selectedProperty(item)\"\n                  >\n                    <v-list-item-title\n                      v-text=\"item.attributes.name\"\n                    ></v-list-item-title>\n                  </v-list-item>\n                </v-list>\n              </v-menu> \n            </template>           \n          </v-col>\n          <v-col lg=\"2\">\n            <v-list-item One-line>\n                <v-list-item-content>\n                    <v-list-item-subtitle>{{ location }} \n                      <v-icon\n                        color=\"grey\"\n                        v-bind=\"attrs\"\n                        v-on=\"on\"\n                      >\n                        mdi-menu-right\n                      </v-icon>    \n                    </v-list-item-subtitle>\n                </v-list-item-content>\n            </v-list-item>         \n          </v-col>        \n      </v-row>\n      <v-spacer></v-spacer>\n      <!-- <v-btn icon>\n        <v-avatar>\n          <v-img src=\"https://gitlab.com/uploads/-/system/user/avatar/3883365/avatar.png\"></v-img>\n        </v-avatar>\n        <Dialog/>\n      </v-btn> -->\n      <v-btn icon class=\"mr-1\">        \n        <Dialog ref=\"dialog\"/>\n      </v-btn>\n    </v-app-bar>\n  </div>  \n</template>\n<script>\nimport Dialog from './Dialog'\n// import Sidebar from './Sidebar'\nexport default {\n    name: 'AppBar',\n    propertyName : '',\n    data () {\n      return {\n        mini : false\n      }\n    },\n    computed : {\n      location() {\n        return this.$route.meta.name\n      },      \n      propertyList() {\n        return this.$store.state.propertyDetails.propertyList\n      },\n      defaultPropertyName() {\n        return this.$store.state.propertyDetails.selectedProperty.attributes.name\n      },\n\n    },\n    created()  {\n      console.log(this.$route.meta.name);\n      this.$nextTick(() => {\n        this.propertyMenuName()\n      })\n    },\n    components : {\n        Dialog,\n        // Sidebar\n    },\n    methods : {\n      selectedProperty(property) {\n        this.$store.dispatch('propertyDetails/selectedProperty', property)\n        .then(\n            console.log(this.$store.state.propertyDetails.selectedProperty),\n            this.propertyMenuName()\n        )\n      },\n      propertyMenuName() {\n        this.propertyName = this.$store.state.propertyDetails.selectedProperty.attributes.name\n      }\n    }\n}\n</script>"]}]}